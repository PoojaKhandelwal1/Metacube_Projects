    </validationRules>
    <validationRules>
        <fullName>ManagedToPeer2Peer</fullName>
        <active>true</active>
        <description>The value cannot be negative.</description>
        <errorConditionFormula>Managed_to_P2P_Min__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Managed_to_P2P_Min__c</errorDisplayField>
        <errorMessage>The value cannot be negative.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUL_Activites</fullName>
        <active>true</active>
        <errorConditionFormula>(RUL_Activities__c &gt; 99) || (RUL_Activities__c &lt; 1) || (RUL_Activities_Contact__c &gt; 99) || (RUL_Activities_Contact__c &lt; 1)</errorConditionFormula>
        <errorMessage>You must specify a number of activities in the period from 1 to 99</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUL_Period</fullName>
        <active>true</active>
        <errorConditionFormula>(RUL_Period__c  &gt; 365) ||(RUL_Period__c  &lt; 1)</errorConditionFormula>
        <errorDisplayField>RUL_Period__c</errorDisplayField>
        <errorMessage>You must specify a number of days in the period from 1 to 365</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RUL_PeriodContact</fullName>
        <active>true</active>
        <description>You must specify a number of days in the period from 1 to 365</description>
        <errorConditionFormula>(RUL_Period_Contact__c&gt; 365) ||(RUL_Period_Contact__c&lt; 1)</errorConditionFormula>
        <errorDisplayField>RUL_Period_Contact__c</errorDisplayField>
        <errorMessage>You must specify a number of days in the period from 1 to 365</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reference_profile_and_Nomination_checked</fullName>
        <active>true</active>
        <description>Reference profile and Nomination both cannot be checked/unchecked</description>
        <errorConditionFormula>OR(AND(Create_Nomination__c, Create_Reference_Profile__c), AND(NOT(Create_Nomination__c), NOT(Create_Reference_Profile__c)))</errorConditionFormula>
        <errorMessage>Reference profile and Nomination both cannot be checked/unchecked.</errorMessage>
    </validationRules>
