            <script>
                var customSettingApp = angular.module('customSettingApp', ['angularMultiSelectList']);
                customSettingApp.controller('customSettingCon', function ($scope, $window, $q) {
                
                    $scope.customSetting = {};
                    $scope.customSettingOld = {};
                    $scope.isFullLicenseAccessible = '{!isFullLicenseAccessible}';
					$scope.IsPageAccessible = {!IsPageAccessible};
                    $scope.isEdit = false;
                    $scope.isEditEnabled = ({!$ObjectType.Custom_Settings__c.updateable});
                    $scope.isError = false;
					$scope.showRolePicklistValues = false;
					$scope.showRoutingGroups = false;
                    $scope.errorMsg = '';
                    $scope.showAccModal = false;
                    $scope.showConModal = false;
                    $scope.isloading = true;
                    $scope.objects = [];
                    $scope.fields = [];
                    $scope.refFieldsContact = [];
					$scope.picklistFieldsRole = [];
                    $scope.refFields = [];
                    $scope.picklistFields = [];
                    $scope.selectedEntries = [];
					$scope.availableEntries = [];
                    $scope.selectedGroups = [];
					$scope.availableGroups = [];
                    
                    $scope.intialFunction = function() {
                   	
                   		if (($scope.isFullLicenseAccessible || $scope.isFullLicenseAccessible == 'true') && ($scope.IsPageAccessible || $scope.IsPageAccessible == 'true')) {
	                   		
		                    $scope.fetchCS().then(function(result) {
		                        $scope.customSetting = result;
		                        $scope.customSettingOld = angular.copy($scope.customSetting);
		                        $scope.isloading = false;
								$scope.showRolePicklistValues = $scope.customSetting.teamRole != null && $scope.customSetting.teamRole != '';
								$scope.showRoutingGroups = $scope.customSetting.teamRouteBy != null && $scope.customSetting.teamRouteBy != '';
		                        
		                        //fetching all the data initially related to attribute
		                        $scope.getObjects().then(function(result) {
		                            $scope.objects = result;
		                            $scope.isloading = false;
									
		                            //get picklist values of Limit to Account /Contact
		                            $scope.getPicklistFields().then(function(result) {
			                            $scope.picklistFields = result;
			                        });
									//get picklist values of Roles to Account 
		                            $scope.getPicklistFieldsRole().then(function(result) {
			                            $scope.picklistFieldsRole = result;
			                        });
									//get Contact Type Fields
			                        $scope.getRefFieldsContact().then(function(result) {
			                            $scope.refFieldsContact = result;
			                        });
			                        //get User Type Fields
			                        $scope.getRefFields().then(function(result) {
			                            $scope.refFields = result;
			                        });

									if ($scope.showRolePicklistValues) {
										//get picklist values of Roles to Account 
										$scope.getRoleEntries($scope.customSetting.teamRole, $scope.customSetting.teamObject, $scope.customSetting.teamRolesToInclude).then(function(result) {
											$scope.roleEntries = result;
											$scope.selectedEntries = [];
											$scope.availableEntries = [];
											
											angular.forEach($scope.roleEntries, function(value) {
												if (value.isSelected == true) {
													$scope.selectedEntries.push(value);
												} else {
													$scope.availableEntries.push(value);
												}
											});
										});
									}

									if ($scope.showRoutingGroups) {
										
										$scope.getRoleEntries($scope.customSetting.teamRouteBy, $scope.customSetting.teamContact == null || $scope.customSetting.teamContact == '' ? $scope.customSetting.teamObject : 'Contact', $scope.customSetting.groupsToExclude).then(function(result) {
											$scope.roleEntries = result;
											$scope.selectedGroups = [];
											$scope.availableGroups = [];
											
											angular.forEach($scope.roleEntries, function(value) {
												if (value.isSelected == true) {
													$scope.selectedGroups.push(value);
												} else {
													$scope.availableGroups.push(value);
												}
											});
										});
									}
		                        });
		                    });
		                    
                            //get picklist values of Limit to Account Contact
                            $scope.getFieldValues().then(function(result) {
	                            $scope.fields = result;
	                        });
	                    } else {
							$scope.isloading = false;
						}
                    }
                    
                    $scope.cancel = function() {
                    	$scope.customSetting = angular.copy($scope.customSettingOld);
                    	$scope.isEdit = false;
						$scope.showRolePicklistValues = $scope.customSetting.teamRole != null && $scope.customSetting.teamRole != '';
						$scope.showRoutingGroups = $scope.customSetting.teamRouteBy != null && $scope.customSetting.teamRouteBy != '';
						$scope.getObjects().then(function(result) {
							$scope.objects = result;
							$scope.isloading = false;
							
							//get picklist values of Limit to Account Contact
							$scope.getPicklistFields().then(function(result) {
								$scope.picklistFields = result;
							});
							//get Contact Type Fields
							$scope.getRefFieldsContact().then(function(result) {
								$scope.refFieldsContact = result;
							});
							//get picklist values of Roles to Account 
							$scope.getPicklistFieldsRole().then(function(result) {
								$scope.picklistFieldsRole = result;
							});
							//get User Type Fields
							$scope.getRefFields().then(function(result) {
								$scope.refFields = result;
							});

							if ($scope.showRolePicklistValues) {
								//get picklist values of Roles to Account 
								$scope.getRoleEntries($scope.customSettingOld.teamRole, $scope.customSetting.teamObject, $scope.customSettingOld.teamRolesToInclude).then(function(result) {
									$scope.roleEntries = result;
									$scope.selectedEntries = [];
									$scope.availableEntries = [];
									
									angular.forEach($scope.roleEntries, function(value) {
										if (value.isSelected == true) {
											$scope.selectedEntries.push(value);
										} else {
											$scope.availableEntries.push(value);
										}
									});
								});
							}

							if ($scope.showRoutingGroups) {
								
								$scope.getRoleEntries($scope.customSettingOld.teamRouteBy, $scope.customSetting.teamContact == null || $scope.customSetting.teamContact == '' ? $scope.customSetting.teamObject : 'Contact', $scope.customSettingOld.groupsToExclude).then(function(result) {
									$scope.roleEntries = result;
									$scope.selectedGroups = [];
									$scope.availableGroups = [];
									
									angular.forEach($scope.roleEntries, function(value) {
										if (value.isSelected == true) {
											$scope.selectedGroups.push(value);
										} else {
											$scope.availableGroups.push(value);
										}
									});
								});
							}
						});
                    }
                    
                    $scope.edit = function() {
                    	$scope.isEdit = true;
                    }

                    $scope.changeObject = function() {
                		$scope.customSetting.teamRouteBy = '';
                		$scope.customSetting.teamUser = '';
                		$scope.customSetting.teamContact = '';
						$scope.customSetting.teamRole = '';
						$scope.customSetting.teamRolesToInclude = '';
						$scope.customSetting.groupsToExclude = '';
						$scope.showRolePicklistValues = false;
						$scope.showRoutingGroups = false;
						$scope.selectedEntries = [];
						$scope.availableEntries = [];
						$scope.selectedGroups = [];
						$scope.availableGroups = [];
                        //get picklist values of Limit to Account Contact
                        $scope.getPicklistFields().then(function(result) {
                            $scope.picklistFields = result;
                        });
						//get picklist values of Roles to Account 
						$scope.getPicklistFieldsRole().then(function(result) {
							$scope.picklistFieldsRole = result;
						});
						//get Contact Type Fields
						$scope.getRefFieldsContact().then(function(result) {
							$scope.refFieldsContact = result;
						});
                        //get User Type Fields
						$scope.getRefFields().then(function(result) {
							$scope.refFields = result;
						});
                    }
					$scope.changeRole = function() {
						$scope.customSetting.teamRolesToInclude = '';
						$scope.showRolePicklistValues = $scope.customSetting.teamRole != null && $scope.customSetting.teamRole != '' && $scope.customSetting.teamRole != '--None--';
		                $scope.selectedEntries = [];
						$scope.availableEntries = [];
								
						if ($scope.showRolePicklistValues) {
							//get picklist values of Roles to Account 
							$scope.getRoleEntries($scope.customSetting.teamRole, $scope.customSetting.teamObject, $scope.customSetting.teamRolesToInclude).then(function(result) {
								$scope.roleEntries = result;
								
								angular.forEach($scope.roleEntries, function(value) {
									if (value.isSelected == true) {
										$scope.selectedEntries.push(value);
									} else {
										$scope.availableEntries.push(value);
									}
								});
							});
						}
                    }
					$scope.changeRouteBy = function() {
						$scope.customSetting.groupsToExclude = '';
						$scope.showRoutingGroups = $scope.customSetting.teamRouteBy != null && $scope.customSetting.teamRouteBy != '' && $scope.customSetting.teamRouteBy != '--None--';
		                $scope.selectedGroups = [];
						$scope.availableGroups = [];
								
						if ($scope.showRoutingGroups) {
							//get picklist values of Roles to Account 
							$scope.getRoleEntries($scope.customSetting.teamRouteBy, $scope.customSetting.teamContact == null || $scope.customSetting.teamContact == '' ? $scope.customSetting.teamObject : 'Contact', $scope.customSetting.groupsToExclude).then(function(result) {
								$scope.roleEntries = result;
								
								angular.forEach($scope.roleEntries, function(value) {
									if (value.isSelected == true) {
										$scope.selectedGroups.push(value);
									} else {
										$scope.availableGroups.push(value);
									}
								});
							});
						}
                    }

					$scope.changeContact = function() {
                		$scope.customSetting.teamUser = '';
						$scope.customSetting.teamRouteBy = '';
						$scope.customSetting.groupsToExclude = '';
                        $scope.selectedGroups = [];
                        $scope.availableGroups = [];
                        $scope.showRoutingGroups = false;
                		
                        //get User Type Fields
						$scope.getRefFields().then(function(result) {
							$scope.refFields = result;
						});
						//get picklist values of Limit to Account Contact
                        $scope.getPicklistFields().then(function(result) {
                            $scope.picklistFields = result;
                        });
                    }
                    
                    $scope.fetchCS = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getCustomSetting}',
                            function(result, event) {
                                result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }

                    $scope.save = function() {
                    
                    	if ($scope.customSetting.adReqRouting && ($scope.customSetting.adRoutingDefault == null || $scope.customSetting.adRoutingDefault == '')) {
                    		$scope.isError = true;
                   			$scope.errorMsg = '{!$Label.Select_Routing}';
                    	} else if ($scope.customSetting.teamObject != null && $scope.customSetting.teamObject != '') {
                    		$scope.customSetting.teamRolesToInclude = '';
							
							angular.forEach($scope.selectedEntries, function(value) {
								$scope.customSetting.teamRolesToInclude += value.id + ',';
							});
							$scope.customSetting.teamRolesToInclude = $scope.customSetting.teamRolesToInclude;

                    		if ($scope.customSetting.teamRouteBy == null || $scope.customSetting.teamRouteBy == '') {
                    			$scope.isError = true;
                   				$scope.errorMsg = '{!$Label.Select_Route_By}';
                    		}
                    		
                    		if ($scope.customSetting.teamUser == null || $scope.customSetting.teamUser == '') {
                    			$scope.isError = true;
                   				$scope.errorMsg = '{!$Label.Select_User_Field}';
                    		}

							if (!$scope.isError && $scope.customSetting.teamRole != null && $scope.customSetting.teamRole != '' && ($scope.customSetting.teamRolesToInclude == null || $scope.customSetting.teamRolesToInclude == '')) {
                    			$scope.isError = true;
                   				$scope.errorMsg = '{!$Label.Select_Role}';
                    		}
                    	}
                    	
                    	if (!$scope.isError) {
                    		$scope.customSetting.groupsToExclude = '';
							
							angular.forEach($scope.selectedGroups, function(value) {
								$scope.customSetting.groupsToExclude = $scope.customSetting.groupsToExclude == ''
                                        ? value.id : $scope.customSetting.groupsToExclude + ',' + value.id;
							});
	                    	$scope.isloading = true;
	                    	$scope.customSetting.teamRolesToInclude = $scope.customSetting.teamRolesToInclude.replace(/,\s*$/, "");
	                    	$scope.saveCS().then(function(result) {
			                	$scope.isloading = false;
			                	
			                	if (result == '') {
			                		$scope.isEdit = false;
			                		$scope.customSettingOld = angular.copy($scope.customSetting);
			                	} else {
			                		$scope.isError = true;
	                    			$scope.errorMsg = result;
			                	}
		                    });
                    	}
                    }
                    
                    //remoteAction call
                    $scope.getObjects = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getObjects}', 
                            function(result, event) {
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
					$scope.getFieldValues = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getFieldValues}', 
                            function(result, event) {
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
                    
                    //remoteAction call
                    $scope.getRefFields = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getReferenceFields}', 
                            $scope.customSetting.teamObject,
							$scope.customSetting.teamContact,
                            function(result, event) {
								result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
					
					//remoteAction call
                    $scope.getRefFieldsContact = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getReferenceFieldsContacts}', 
                            $scope.customSetting.teamObject,
                            function(result, event) {
								result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
					//remoteAction call
                    $scope.getRoleEntries = function(picklistField, selectedObject, selectedValues) {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getRoleEntries}', 
                            picklistField, selectedObject, selectedValues,
                            function(result, event) {
								result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
					}

					$scope.parseResult = function(result){
                        if (result != null) {
                            result = result.replace(/&quot/gi, '"');
                            result = result.replace(/&amp/g, '&');
                            result = result.replace(/&lt/g, '<');
                            result = result.replace(/&gt/g, '>');
                            result = result.replace(/;/g, '');
                            result = result.replace(/&#39/g, '\'');
                        }
                        return result;
                    }

					//remoteAction call
                    $scope.getPicklistFieldsRole = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getPicklistFieldsRole}', 
                            $scope.customSetting.teamObject,
                            function(result, event) {
								result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }

                    //remoteAction call
                    $scope.getPicklistFields = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.getPicklistFields}', 
                            $scope.customSetting.teamObject,
							$scope.customSetting.teamContact,
                            function(result, event) {
								result = $scope.parseResult(result);
								result = JSON.parse(result);
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
                    $scope.saveCS = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.AdvRoutingExtensionsSLDS.saveCSRUL}', angular.toJson($scope.customSetting),
                            function(result, event) {
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }

                    $scope.parseResult = function(result) {
					
                        if (result != null) {
                            result = result.replace(/&quot/gi, '"');
                            result = result.replace(/&amp/g, '&');
                            result = result.replace(/&lt/g, '<');
                            result = result.replace(/&gt/g, '>');
                            result = result.replace(/&#39/g, '\'');
                            result = result.replace(/;/g, '');
                        }
                        return result;
                    }
                });
            </script>   
        </head>
        <body ng-app="customSettingApp" class="slds">
        	<div class="container">
	            <form id="frmId" class="slds-m-left--small" ng-controller="customSettingCon" ng-cloak="ng-cloak">
	                <div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_info" style="text-align: center;background-color: #706e6b !important;" role="alert" ng-show="isFullLicenseAccessible && !IsPageAccessible" ng-cloak="true">
						<span class="slds-icon_container slds-icon-utility-user slds-m-right_x-small">
							<svg class="slds-icon slds-icon_x-small" aria-hidden="true" style="width: 1rem;margin-right: .75rem !important;">
								<use xlink:href="{!URLFOR($Resource.LightningCSS_2_3, '/assets/icons/utility-sprite/svg/symbols.svg#info')}"></use>
							</svg>
						</span>
						<h2 style="display: inline;font-size: 0.8125rem !important;">{!$Label.Insufficient_Privileges}</h2>
					</div>
					<div class="slds-notify slds-notify_alert slds-theme_alert-texture slds-theme_info" style="text-align: center;background-color: #706e6b !important;" role="alert" ng-show="!isFullLicenseAccessible" ng-cloak="true">
						<span class="slds-icon_container slds-icon-utility-user slds-m-right_x-small" >
							<svg class="slds-icon slds-icon_x-small" aria-hidden="true" style="width: 1rem;margin-right: .75rem !important;">
								<use xlink:href="{!URLFOR($Resource.LightningCSS_2_3, '/assets/icons/utility-sprite/svg/symbols.svg#info')}"></use>
