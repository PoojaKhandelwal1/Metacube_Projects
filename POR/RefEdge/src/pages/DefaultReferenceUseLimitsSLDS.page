            <script>
                var customSettingApp = angular.module('customSettingApp', []);
                customSettingApp.controller('customSettingCon', function ($scope, $window, $q) {
                
                    $scope.customSetting = {};
                    $scope.customSettingOld = {};
                    $scope.isFullLicenseAccessible = '{!isFullLicenseAccessible}';
                    $scope.isEdit = false;
                    $scope.isEditEnabled = ({!$ObjectType.Custom_Settings__c.updateable});
                    $scope.isError = false;
                    $scope.errorMsg = '';
                    $scope.showAccModal = false;
                    $scope.showConModal = false;
                    $scope.isloading = true;
                    
                    $scope.intialFunction = function() {
                   	
                   		if ($scope.isFullLicenseAccessible || $scope.isFullLicenseAccessible == 'true') {
	                   		
		                    $scope.fetchCS().then(function(result) {
		                        $scope.customSetting = result;
		                        $scope.customSettingOld = angular.copy($scope.customSetting);
		                        $scope.isloading = false;
		                    });
	                    }
                    }
                    
                    $scope.cancel = function() {
                    	$scope.customSetting = angular.copy($scope.customSettingOld);
                    	$scope.isEdit = false;
                    }
                    
                    $scope.fetchCS = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.ChangeAllNLNRequestsControllerSLDS.getCustomSetting}',
                            function(result, event) {
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }
                    
                    $scope.showAccPopup = function() {

                    	if ($scope.customSetting.accountRULPeriod != $scope.customSettingOld.accountRULPeriod) {
                    		$scope.showAccModal = true;
                    	} else {
                    		$scope.showConPopup();
                    	}
                    }
                    
                    $scope.showConPopup = function() {
						$scope.showAccModal = false;
						
                    	if ($scope.customSetting.contactRULPeriod != $scope.customSettingOld.contactRULPeriod) {
                    		$scope.showConModal = true;
                    	} else {
                    		$scope.save();
                    	}
                    }
                    
                    $scope.save = function() {
                    	$scope.showConModal = false;
                    	$scope.isloading = true;
                    	
                    	$scope.saveCS().then(function(result) {
		                	console.log('saved');
		                	$scope.isloading = false;
		                	
		                	if (result == '') {
		                		$scope.isEdit = false;
		                		$scope.customSetting.updateAllAccount = false;
    							$scope.customSetting.updateAllContact = false;
		                		$scope.customSettingOld = angular.copy($scope.customSetting);
		                	} else {
		                		$scope.isError = true;
                    			$scope.errorMsg = result;
		                	}
	                    });
                    }
                    
                    $scope.saveCS = function() {
                        var deferred = $q.defer();
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.ChangeAllNLNRequestsControllerSLDS.saveCSRUL}', angular.toJson($scope.customSetting),
                            function(result, event) {
                                deferred.resolve(result);
                            },
                            {escape: true}
                        );
                        return deferred.promise;
                    }

                    $scope.parseResult = function(result) {
					
                        if (result != null) {
                            result = result.replace(/&quot/gi, '"');
                            result = result.replace(/&amp/g, '&');
                            result = result.replace(/&lt/g, '<');
                            result = result.replace(/&gt/g, '>');
                            result = result.replace(/&#39/g, '\'');
                            result = result.replace(/;/g, '');
                        }
                        return result;
                    }
                });
            </script>   
