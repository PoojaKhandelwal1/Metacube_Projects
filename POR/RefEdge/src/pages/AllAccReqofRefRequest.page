<apex:page standardController="Reference_Request__c" sidebar="false" id="pageId" extensions="AllAccReqofRefRequestController" standardStylesheets="true" showHeader="false" applyHtmlTag="true" applyBodyTag="false" docType="html-5.0">
	<apex:outputPanel rendered="{!!isSLDS}">
	    <apex:pageMessage summary="{!$Label.User_License}" strength="3" severity="Info" rendered="{!NOT(isFullLicenseAccessible)}" />
	    <apex:form rendered="{!isFullLicenseAccessible}">
	        <apex:pageblock >
	            <apex:pageMessage severity="Info" strength="2" summary="{!$Label.No_Records_Found}" rendered="{!requestList.size <=0}"/>  
	            <apex:pageBlockTable value="{!requestList}" var="accreq" rendered="{!requestList.size > 0}">
	                <apex:column headerValue="{!$Label.Account}">
	                	<apex:commandLink value="{!accreq.Account__r.Name}" onclick="window.open('/{!accreq.Account__c}');return false;"/>
	                </apex:column>
	                <apex:column headerValue="{!$Label.Reference_Type_Needed}">
	                	<apex:commandLink value="{!accreq.Reference_Type_Needed__r.Name}" onclick="window.open('/{!accreq.Reference_Type_Needed__c}');return false;"/>
	                </apex:column>
	                <apex:column headerValue="{!$ObjectType.Reference_Request_Account__c.fields.Requester__c.Label}">
	                	<apex:commandLink value="{!accreq.Requester__r.Name}" onclick="window.open('/{!accreq.Requester__c}');return false;"/>
	                </apex:column>
	                <apex:column value="{!accreq.Deadline__c}"/>
	                <apex:column value="{!accreq.Managed_Request__c}"/>
	                <apex:column value="{!accreq.Approval_Status__c}"/>
	                <apex:column value="{!accreq.Account_Request_Status__c}"/>
	                <apex:column headerValue="{!$Label.Details}">
	                    <apex:commandLink value="{!accreq.Name}" onclick="window.open('/{!accreq.Id}');return false;"/>
	                </apex:column>
	            </apex:pageBlockTable>
	        </apex:pageblock>
	    </apex:form>
    </apex:outputPanel>
    <apex:include pageName="AllAccReqofRefRequestSLDS" rendered="{!isSLDS}"/>
</apex:page>