    <script>
        function jsCreateGroup(publicOrPrivate) {
            var chatterGroupName  = prompt("{!$Label.Create_a_Group_Message}");
            if (chatterGroupName != null) {
                if(publicOrPrivate == 'Public')
                    createPublicGroup(chatterGroupName);
                PORShowLoading();
            }
        }
        
        function jsAlertProfile(proName){
            var message = '{!$Label.Profile_alert_message}';
            alert(message.replace('@@@', proName));
        }
        
        // added for #498 - method to create remote site setting
        function jsCreateRemoteSiteSetting() {
        	sforce.connection.remoteFunction({
                url: 'https://' + window.location.host + '/services/Soap/m/32.0',
                onSuccess: function(response, xhr) {
                    window.location.reload(true);
                },
                onFailure: function(response, xhr) {
                    alert(response);
                },
                method: 'POST',
                async: false,
                requestHeaders: {
                    'Content-Type': 'text/xml',
                    'SOAPAction': '""'
                },
                requestData: '{!JSENCODE(DeployXml)}',
                cache: false,
                timeout: 10000
            });
        }
    </script>
