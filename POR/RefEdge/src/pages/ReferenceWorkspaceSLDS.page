							newRequest['Request Name'] = '';
							newRequest['isUR'] = data['isUR'];
							data['Account__c'] = $scope.originalCopyForEditedData[data['Id']].Account__c;
							data[item.fieldLabel] = $scope.originalCopyForEditedData[data['Id']][item.fieldLabel];
