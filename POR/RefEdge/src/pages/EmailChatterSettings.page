                }
            }
        }
        
        function checkForSlackEnabled(clickObj) {
            var globalNotificationSettingsSectionid = 'pageId:fromId:pbId:sec0';
            var globalSlackInput = document.getElementById(globalNotificationSettingsSectionid).children[1].querySelectorAll('table > tbody > tr > :nth-child(4) input[type=checkbox]');
            
            if (document.getElementById(clickObj).checked) {
                if (globalSlackInput.length > 0 && !globalSlackInput[0].checked) {
