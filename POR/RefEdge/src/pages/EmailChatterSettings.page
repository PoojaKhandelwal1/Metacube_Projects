        function saveJS() {
            saveMe();
            return false;
        }

        function openEditNotificationModalJS(isPreview) {

            if (isPreview) {

                jQuery("#notificationModal").dialog({
                    width: '600px',
                    modal: true,
                    resizable: false,
                    draggable: false,
                    buttons: [{
                        text: "{!$Label.Done}",
                        "class": "btn",
                        click: function() {
                            jQuery(this).dialog("close");
                            return false;
                        }
                    }],
                    close: function(event, ui) {
                    	jQuery('.ui-dialog:has(#notificationModal)').empty().remove();
                        closeEditNotificationModal();
                    }
                });
            } else {

                jQuery("#notificationModal").dialog({
                    width: '900px',
                    modal: true,
                    resizable: false,
                    draggable: false,
                    buttons: [{
                        text: "Reset",      // TODO : create label
                        "class": "btn",
                        click: function() {
                            PORShowLoading();
                            resetNotification();
                            jQuery(this).dialog("close");
                            return false;
                        }
                    }, {
                        text: "{!$Label.Cancel}",
                        "class": "btn",
                        click: function() {
                            jQuery(this).dialog("close");
                            return false;
                        }
                    }, {
                        text: "{!$Label.Save}",
                        "class": "btn",
                        click: function() {
                            PORShowLoading();
                            var subjectVal = document.getElementById('pageId:fromId:pbId:subject').value;
                            var editableBodyVal = document.getElementById('pageId:fromId:pbId:editableBody').value;
                            saveNotification(subjectVal, editableBodyVal);
                            jQuery(this).dialog("close");
                            return false;
                        }
                    }],
                    close: function(event, ui) {
                    	jQuery('.ui-dialog:has(#notificationModal)').empty().remove();
                        closeEditNotificationModal();
                    }
                });
            }
        }

        var lookUpWindowOpen = false;
        var popupflag = false;
        var newWin = null;
        var imageName = '';

        function openLookupWindow(imageNameTemp) {

            if (!lookUpWindowOpen) {
                imageName = imageNameTemp;
                lookUpWindowOpen = true;
                popupflag = true;
                var name = '';

                if (imageName == 'header') {
                    name = document.getElementById('pageId:fromId:pbId:sec00:headerImageSection:headerDocName').value;
                } else if (imageName == 'footer') {
                    name = document.getElementById('pageId:fromId:pbId:sec00:footerImageSection:footerDocName').value;
                }
                newWin = window.open(encodeURI('{!$Page.CustomLookup}?strText=' + name + '&obj=document'), 'Popup', 'height=500, width=700, left=100, top=100, resizable=no, scrollbars=yes, toolbar=no, status=no');
                lookUpWindowOpen = false;
            }
        }

        function closePopup(value, label) {

            if (newWin != null) {
                newWin.close();

                if (typeof label != 'undefined') {

                    if (popupflag) {

                        if (imageName == 'header') {
                            document.getElementById('pageId:fromId:pbId:sec00:headerImageSection:headerDocName').value = label;
                            document.getElementById('pageId:fromId:pbId:sec00:headerImageSection:headerDocId').value = value;
                        } else if (imageName == 'footer') {
                            document.getElementById('pageId:fromId:pbId:sec00:footerImageSection:footerDocName').value = label;
                            document.getElementById('pageId:fromId:pbId:sec00:footerImageSection:footerDocId').value = value;
                        }
                        popupflag = false;
                    }
                }
            }
        }
    </script>

