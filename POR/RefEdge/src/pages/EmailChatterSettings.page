    <script type="text/javascript">
        var sections = 9;
        PORShowLoading();
        setTimeout(function() {
            PORHideLoading();
        }, 1500);
        
        function isDisableGlobalSetting(notificationType) {
        	var checkboxInputColIndex = (notificationType == 'email') ? 2 : ((notificationType == 'chatter') ? 3 : ((notificationType == 'slack') ? 4 : ''));
        	var flag = true;
        	if(checkboxInputColIndex != '') {
                for (var i = 1; i <= sections; i++) {
                	var id = 'pageId:fromId:pbId:sec' + i;
                	var inputElements = document.getElementById(id).children[1].querySelectorAll('table > tbody > tr > :nth-child(' + checkboxInputColIndex + ') input[type=checkbox]');
                	
                	for (var j = 0; j < inputElements.length; j++) {
                		if(inputElements[j].checked) {
                			flag = false;
                			break;
                		}
                	}
                	
                	if(!flag) {
                		break;
                	}
                }
                return flag;
        	}  
        	return false;
        }
		
        function checkAllEmail(compValue, clickObj) {
            
            if (compValue.checked) {
                var result = confirm("{!$Label.EmailEnabled_check_message}");
                
                if (result) {
                    document.getElementById(clickObj).checked = true;
                    
                    for (var i = 1; i <= sections; i++) {
                        var id = 'pageId:fromId:pbId:sec'+i;
                        
                        var emailInputs = document.getElementById(id).children[1].querySelectorAll('table > tbody > tr > :nth-child(2) input[type=checkbox]');
                        for (var j = 0; j < emailInputs.length; j++) {
                            emailInputs[j].checked = true;
                        }
                    }
                } else {
                    document.getElementById(clickObj).checked = false;
                }
            } else {
                var result = confirm("{!$Label.EmailEnabled_uncheck_message}");
                
                if (result) {
                    document.getElementById(clickObj).checked = false;
                    
                    for (var i = 1; i <= sections; i++) {
                        var id = 'pageId:fromId:pbId:sec' + i;
                        var emailInputs = document.getElementById(id).children[1].querySelectorAll('table > tbody > tr > :nth-child(2) input[type=checkbox]');
                        for (var j = 0; j < emailInputs.length; j++) {
                            emailInputs[j].checked = false;
                        }
                    }
                } else {
                    document.getElementById(clickObj).checked = true;
                }
            }
        }
        
        function checkForEmailEnabled(clickObj) {
            var globalNotificationSettingsSectionid = 'pageId:fromId:pbId:sec0';
            var globalEmailInput = document.getElementById(globalNotificationSettingsSectionid).children[1].querySelectorAll('table > tbody > tr > :nth-child(2) input[type=checkbox]');
            
            if (document.getElementById(clickObj).checked) {
                if (globalEmailInput.length > 0 && !globalEmailInput[0].checked) {
                    var result = confirm("{!$Label.EmailEnabled_set_ON_message}");
                    
                    if (result) {
                        document.getElementById(clickObj).checked = true;
                        globalEmailInput[0].checked = true;
                    } else {
                        document.getElementById(clickObj).checked = false;
                    }
                }
            } else {
            	if(isDisableGlobalSetting('email')) {
            		globalEmailInput[0].checked = false;
            	}
            }
        }
        
