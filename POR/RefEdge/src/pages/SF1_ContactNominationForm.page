<apex:page standardController="Contact" sidebar="false" extensions="InlinePageNavigationController">
    <apex:includeScript value="/support/console/44.0/integration.js" />

    <script>
        function openNominationForm(result) {
            var primaryTabId = result.id;
            sforce.console.openSubtab(primaryTabId, '{!$Page.NominationFormSLDS}?AccountId={!accountId}&ContactId={!contactId}&isSLDS=true', true, 'Contact Nomination', null, null, 'Contact Nomination');
        }

        if ((typeof sforce != 'undefined') && sforce != null && sforce.console.isInConsole()) {
            sforce.console.getEnclosingPrimaryTabId(openNominationForm);
        } else if ((typeof sforce != 'undefined') && sforce != null) {

            if (Object.keys(sforce)[0] == 'console' && Object.keys(sforce).length == 1) {
                window.location.href = '{!$Page.NominationFormSLDS}?AccountId={!accountId}&ContactId={!contactId}&isSLDS=false';
            } else {
                sforce.one.navigateToURL('{!$Page.NominationFormSLDS}?AccountId={!accountId}&ContactId={!contactId}&isSLDS=true');
            }
        } else {
            window.location.href = '{!$Page.NominationFormSLDS}?AccountId={!accountId}&ContactId={!contactId}&isSLDS=false';
        }
    </script>
</apex:page>