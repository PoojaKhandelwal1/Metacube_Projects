<apex:page standardController="Reference_Request_Account__c" extensions="SelectRefUseContactsController" id="pageId" sidebar="false"   standardStylesheets="false"   applyHtmlTag="true" applyBodyTag="false" docType="html-5.0">
    <apex:stylesheet value="{!URLFOR($Resource.JqueryFiles, 'jquery-ui-1.9.2.custom.min.css')}"/>
    <apex:includeScript value="{!URLFOR($Resource.Jplayer, 'Jplayer/js/jquery.jplayer.min.js')}"/>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script>
	    jQuery(window).load(function() {
           reset(true);
		});
	</script>
    <apex:form id="formId" rendered="{!isFullLicenseAccessible}">
        <apex:actionFunction name="reset" action="{!reset}" reRender="newPanelId">
            <apex:param name="isSLDS" assignTo="{!isSLDS}" value=""/>
        </apex:actionFunction>
    </apex:form>
</apex:page>