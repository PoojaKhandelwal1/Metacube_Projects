<apex:page renderAs="pdf" showHeader="false"  sidebar="false" standardstylesheets="false"  applyHtmlTag="false" docType="html-5.0" applyBodyTag="false" controller="DealDocumentPrintCtrl">
    <body style="font-size: font-size: 14px;;font-family: Arial Unicode MS,sans-serif; margin-top:0px;">
    <!-- Start: company info Section -->
        <div style="{!if(companyLogoURL != null, 'height:130px', 'height:70px')};width:800px;margin-bottom:30px;">
            <apex:outputPanel rendered="{!companyLogoURL != null}" layout="none">
                <div style="width:135px;float: left;">
                    <img height="130px;" src="{!(companyLogoURL)}" width="130px;"/>
                </div>
            </apex:outputPanel>
            <div style="width:300px;float: left;"> 
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;text-transform: capitalize;" value="{!dealDocumentDetailRecord.CompanyInfo.CompanyName}"></apex:outputText> <br/>
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" value="{!dealDocumentDetailRecord.CompanyInfo.Address1} {!dealDocumentDetailRecord.CompanyInfo.Address2}"></apex:outputText><br/>
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!dealDocumentDetailRecord.CompanyInfo.City != null}" value="{!dealDocumentDetailRecord.CompanyInfo.City} ,"></apex:outputText>
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" value="{!dealDocumentDetailRecord.CompanyInfo.State} , {!dealDocumentDetailRecord.CompanyInfo.PostalCode}"></apex:outputText>
                <br/>
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" value="{!dealDocumentDetailRecord.CompanyInfo.BusinessPhone}"></apex:outputText> &nbsp;&nbsp;
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" value="{!dealDocumentDetailRecord.CompanyInfo.BusinessEmail}"></apex:outputText><br/>
                <apex:outputText style="font-size: 14px;font-family: Arial Unicode MS,sans-serif;" value="{!dealDocumentDetailRecord.CompanyInfo.TaxId}"></apex:outputText>
            </div>
            <div style="width: 250px;{!if(companyLogoURL == null, 'padding-left:120px', 'padding-left:0px')};font-family: Arial Unicode MS,sans-serif;float:left;font-size:14px;">
                <apex:outputPanel rendered="{!documentType != null && documentType != ''}" layout="none">
                    <apex:outputPanel rendered="{!documentType == 'Offer to purchase'}" layout="none">
                        <span style="float:right;margin-right:10px;">O F F E R<span style="padding-left:10px;padding-right:10px;"> T O</span>P U R C H A S E</span>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!documentType != null && documentType != '' && (documentType != 'Bill of sale' || (documentType == 'Bill of sale' &&  !isDisplayTaxInvoice))}" layout="none">
                        <span style=" float:right;margin-right:10px;">B I L L<span style="padding-left:10px;padding-right:10px;"> O F</span>S A L E</span>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!documentType != null && documentType != '' && (documentType == 'Bill of sale' &&  isDisplayTaxInvoice)}" layout="none">
                        <span style=" float:right;margin-right:10px;">T A X<span style="padding-left:10px;padding-right:10px;">I N V O I C E</span></span>
                    </apex:outputPanel>
                </apex:outputPanel>
                <apex:outputPanel rendered="{!documentType == '' || documentType == null}" >
                    <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.Status != 'Invoiced'}" layout="none">
                        <span style="float:right;margin-right:10px;">O F F E R<span style="padding-left:10px;padding-right:10px;"> T O</span>P U R C H A S E</span>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.Status == 'Invoiced'}" layout="none">
                        <span style=" float:right;margin-right:10px;">B I L L<span style="padding-left:10px;padding-right:10px;"> O F</span>S A L E</span>
                    </apex:outputPanel>
                </apex:outputPanel>
            </div>
        </div>
        <!-- End : Company info section --> 
        <!-- Start : sold to section -->
       <div style="width:800px;min-height:110px;font-family: Arial Unicode MS,sans-serif;padding-bottom:20px;">
            <div style="width:500px;float: left;">
                <div style="width:320px;">
                    <div style="width:360px;{!if(companyLogoURL == null, 'margin-top:15px', 'margin-top:0px')};font-size: 14px;font-weight: bold;font-family: Arial Unicode MS,sans-serif;border-bottom:1px solid black;">
                        <apex:outputPanel rendered="{!documentType != 'Bill of sale'}"><apex:outputText > SOLD TO</apex:outputText></apex:outputPanel>
                        <apex:outputPanel rendered="{!documentType == 'Bill of sale' && !isDisplayTaxInvoice}"><apex:outputText >SOLD TO</apex:outputText></apex:outputPanel>
                         <apex:outputPanel rendered="{!documentType == 'Bill of sale' && isDisplayTaxInvoice}"><apex:outputText >INVOICED AND DELIVERED TO</apex:outputText></apex:outputPanel>
                    </div>
                    <apex:outputPanel style="font-size: 14px;" layout="none" > 
                <div style="width:390px;float: left;font-family: Arial Unicode MS,sans-serif;margin-top:10px;margin-bottom:8px;">   
                    <div style="width:220px;"></div>
                        <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerName != null}" layout="none">
                            <apex:outputText style="font-size:14px;padding-top:10px;font-weight: bold;" value="{!dealDocumentDetailRecord.DealInfo.CustomerName}"></apex:outputText><br/>
                            </apex:outputPanel>
                           <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerAddress != null}" layout="none">
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerAddress}"></apex:outputText>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerAddress2 != null}" layout="none">
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerAddress2} "></apex:outputText>
                            </apex:outputPanel>
                           
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerCityProv != null}" layout="none">
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerCityProv} "></apex:outputText><br/>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerState != null}" layout="none">
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerState}"></apex:outputText><br/>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerPostal != null}" layout="none">
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerPostal}"></apex:outputText><br/>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerPhone != null}" layout="none">
                                <span style="font-size:14px;font-weight: bold;">Phone:</span>
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerPhone}"></apex:outputText><br/>
                            </apex:outputPanel>
                            <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.CustomerEmail != null}" layout="none">
                                <span style="font-size:14px;font-weight: bold;">Email:</span>
                                <apex:outputText style="font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.CustomerEmail}"></apex:outputText><br/> 
                            </apex:outputPanel>
                            </div>
                         </apex:outputPanel>
                </div>
            </div>
            <apex:outputPanel rendered="{!documentType == null || documentType == ''}" layout="none">
                <div style="width:60px;float:left;margin-top: 45px;font-family: Arial Unicode MS,sans-serif;">
                    <apex:outputLabel style="width:120px;margin-bottom:10px; float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.Status == 'Invoiced'}" value="Invoice #:"></apex:outputLabel>
                    <apex:outputLabel style="width:120px;margin-bottom:10px; float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.Status != 'Invoiced'}" value="Order #:"></apex:outputLabel>
                    <br/>
                    <apex:outputLabel style="width:120px;margin-bottom:10px;float:left;font-size:14px;" value="Date:"></apex:outputLabel><br/>
                    <apex:outputLabel style="width:120px;margin-bottom:10px;float:left; font-size:14px;" value="Sold By:"></apex:outputLabel>
                </div>
                <div style="width:200px;float: left;margin-top: 45px;font-family: Arial Unicode MS,sans-serif;">
                    <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.Status == 'Invoiced'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.InvoiceNumber != null}" value="{!dealDocumentDetailRecord.DealInfo.InvoiceNumber}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.OrderNumber != null}" value="{!dealDocumentDetailRecord.DealInfo.OrderNumber}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.Status == 'Invoiced'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.InvoiceDate != null}" value="{!dealDocumentDetailRecord.DealInfo.InvoiceDate}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealInfo.Status != 'Invoiced'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.OrderDate != null}" value="{!dealDocumentDetailRecord.DealInfo.OrderDate}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.SoldBy != null}" value="{!dealDocumentDetailRecord.DealInfo.SoldBy}"></apex:outputText><br/>
                </div>
            </apex:outputPanel>
            <apex:outputPanel rendered="{!documentType != null && documentType != ''}" layout="none">
                <div style="width:60px;float:left;margin-top: 45px;font-family: Arial Unicode MS,sans-serif;">
                    <apex:outputLabel style="width:120px;margin-bottom:10px; float:left;font-size:14px;" value="Order #:"></apex:outputLabel>
                    <apex:outputLabel style="width:120px;margin-bottom:10px; float:left;font-size:14px;" rendered="{!documentType == 'Bill of sale'}" value="Invoice #:"></apex:outputLabel>
                    <br/>
                    <apex:outputLabel style="width:120px;margin-bottom:10px;float:left;font-size:14px;" value="Date:"></apex:outputLabel><br/>
                    <apex:outputLabel style="width:120px;margin-bottom:10px;float:left; font-size:14px;" value="Sold By:"></apex:outputLabel>
                </div>
                <div style="width:200px;float: left;margin-top: 45px;font-family: Arial Unicode MS,sans-serif;">
                    <apex:outputPanel layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" value="{!dealDocumentDetailRecord.DealInfo.OrderNumber}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!documentType == 'Bill of sale'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!documentType == 'Bill of sale'}" value="{!IF(dealDocumentDetailRecord.DealInfo.InvoiceNumber != null || dealDocumentDetailRecord.DealInfo.InvoiceNumber != '', dealDocumentDetailRecord.DealInfo.InvoiceNumber, dealDocumentDetailRecord.DealInfo.OrderNumber)}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!documentType == 'Bill of sale'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!documentType == 'Bill of sale'}" value="{!dealDocumentDetailRecord.DealInfo.InvoiceDate}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!documentType != 'Bill of sale'}" layout="none">
                        <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!documentType != 'Bill of sale'}" value="{!dealDocumentDetailRecord.DealInfo.OrderDate}"></apex:outputText><br/>
                    </apex:outputPanel>
                    <apex:outputText style="margin-left:10px;width:200px; margin-bottom:10px;float:left;font-size:14px;" rendered="{!dealDocumentDetailRecord.DealInfo.SoldBy != null}" value="{!dealDocumentDetailRecord.DealInfo.SoldBy}"></apex:outputText><br/>
                </div>
            </apex:outputPanel>
        </div>
        <!-- End : sold to section -->
        <apex:variable value="{!0}" var="unitIndex"/>
        <apex:variable value="{!0}" var="index1"/>
        <apex:variable value="{!0}" var="index2"/>
        <apex:variable value="{!0}" var="index4"/>
        <apex:variable value="{!0}" var="index6"/>
        <apex:variable value="{!0}" var="index7"/>
        <apex:variable value="{!0}" var="index8"/>
         <!-- Start : Unit section -->
         <apex:outputPanel rendered="{!dealDocumentDetailRecord.DealDetailWrapperObj.UnitList.size > 0}" layout="none">
            <apex:repeat value="{!dealDocumentDetailRecord.DealDetailWrapperObj.UnitList}" var="unitDeal">
                <apex:variable var="unitIndex" value="{!unitIndex + 1}"/>
                <table style="width:700px;margin-top:50px;float:left;">
                    <thead>
                        <tr>
                            <th style="border-bottom: 1px solid black;">
                                <div style="width:300px;font-weight: bold;display: inline-block;text-transform: uppercase;font-size:14px;">Unit Purchased - 
                                    <apex:outputPanel rendered="{!unitDeal.ItemDesc.COUId != null}" layout="none"><span>{!unitDeal.ItemDesc.UnitNumber}</span> </apex:outputPanel>
                                    <apex:outputPanel rendered="{!unitDeal.ItemDesc.COUId == null}" layout="none"><span>Temporary unit {!unitIndex}</span></apex:outputPanel> 
                                </div>
                                <apex:outputPanel rendered="{!unitDeal.ItemDesc.VIN !=''}" layout="none">
                                    <div style="width:345px;text-align:right;display: inline-block;text-transform: uppercase;font-size:14px;">VIN: {!unitDeal.ItemDesc.VIN}
                                    </div>
                                </apex:outputPanel>
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        
                        <tr>
                            <td>
                               <table style="width:700px;">
                                    <tbody>
                                        <tr>
                                            <td style="width:60px;font-size: 14px;">{!unitDeal.ItemDesc.Year}</td>
                                            <td style="width:400px;font-size: 14px;text-transform: capitalize;">{!unitDeal.ItemDesc.UnitName}</td>
                                            <td style="width:30px;"></td>
                                            <td style="width:200px;font-size: 14px;text-transform: capitalize;">{!unitDeal.ItemDesc.Colour}</td>
                                            <td style="width:150px;font-size: 14px;">{!unitDeal.ItemDesc.Mileage}</td>
                                        </tr>
                                    </tbody>
                                </table>
                             </td>
                        </tr>
                    </tbody> 
                </table>
                <div style="margin-left:68px;width:650px;float:left;">
                    <apex:repeat value="{!unitDeal.ItemDesc.NotesList}" var="dealNotes">
                        <div style="font-size: 14px;">{!dealNotes}</div>
                    </apex:repeat>
                </div>
                <!-- START: UnitDetails -->
                <div style="width:600px;margin-left:50px;margin-top:15px;float:left;">
                    <apex:outputPanel rendered="{!unitDeal.DealUnitPriceCost.size > 0}" layout="none">
                        <div>
                            <div style="border-bottom: 1px solid black;margin-left:19px;font-size: 14px;margin-bottom: 10px;">Unit Details
                            </div>
                            <table style="width:600px;">
                                <apex:repeat value="{!unitDeal.DealUnitPriceCost}" var="dealUnitPrice">
                                    <apex:variable var="index1" value="{!index1 + 1}"/>
                                    <tbody>
                                        <tr>
                                            <td style="min-width:37px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="min-width:455px;font-size: 14px;padding-bottom: 10px;">{!dealUnitPrice.ItemDescription}</td>
                                            
                                            <td style="min-width:58px;font-size: 14px;text-align:right;padding-bottom: 10px;">
                                                <apex:outputText value="{0, number, currency}">
                                                    <apex:param value="{!dealUnitPrice.TotalPrice}"/>
                                                </apex:outputText>
                                            </td>
                                            <td style="font-weight: bold;font-size: 14px;min-width:58px;text-align: left;padding-bottom: 10px;">
                                                <apex:outputPanel rendered="{!unitDeal.DealUnitPriceCost.size == index1}" layout="none">
                                                    <apex:outputText value="{0, number, currency}">
                                                    <apex:param value="{!unitDeal.UnitAndPriceCost}"/>
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                            </td>
                                            <td style="min-width:30px;padding-bottom: 10px;"></td>
                                        </tr>
                                    </tbody>
                                </apex:repeat>
                            </table>
                        </div>
                    </apex:outputPanel>
                </div>
                <!-- End: UnitDetails -->
                <!-- START: Installed Options -->
                <div style="width:600px;margin-left:50px;margin-top:15px;float:left;">
                     <apex:outputPanel rendered="{!unitDeal.InstalloptionAndFeeList.size > 0}" layout="none">
                        <div style="border-bottom: 1px solid black;margin-left:19px;margin-bottom: 10px;">Installed Options
                        </div>
                        <table style="width:700px;">
                            <apex:repeat value="{!unitDeal.InstalloptionAndFeeList}" var="installoptionAndFee">
                                <apex:variable var="index2" value="{!index2 + 1}"/>
                                <apex:variable var="index3" value="{!0}"/>
                                <tbody>
                                    <apex:outputPanel rendered="{!installoptionAndFee.hasChildren}" layout="none">
                                        <tr>
                                            <td style="width:37px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="width:150px;font-size: 14px;padding-bottom: 10px;">{!installoptionAndFee.ItemDescription}</td><!--   -->
                                            <td style="width:30px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="width:50px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="width:20px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="width:20px;font-size: 14px;padding-bottom: 10px;"></td>
                                            <td style="width:105px;font-size: 14px;padding-bottom: 10px;text-align: right;"><!-- padding-left:45px; --> 
                                                <apex:outputText value="{0, number, currency}">
                                                    <apex:param value="{!installoptionAndFee.Price}"/>
                                                </apex:outputText>
                                            </td>
                                            <td style="width:88px;padding-bottom: 10px;padding-right:30px;"></td>
                                        </tr>
                                    </apex:outputPanel> 
                                        <apex:repeat value="{!installoptionAndFee.OptionAndFeeList}" var="installoptionFee">
                                            <apex:variable var="index3" value="{!index3 + 1}"/>
                                            <tr>
                                                 <td style="width:37px;padding-bottom: 10px;"></td>
                                                 <td style="width:150px;font-size: 14px;padding-bottom: 10px;{!if(installoptionAndFee.hasChildren, 'padding-left:30px', 'padding-left:0px')};">{!installoptionFee.Item}</td>
                                                 <td style="width:30px;padding-bottom: 10px;"></td>
                                                 <td style="width:50px;font-size: 14px;padding-bottom: 10px;">
                                                    <apex:outputText value="{0, number, ###}">
                                                            <apex:param value="{!installoptionFee.Qty}" />
                                                    </apex:outputText>
                                                 </td>
                                                 <td style="width:20px;padding-bottom: 10px;font-size: 14px;">@</td>
                                                 <td style="width:20px;padding-bottom: 10px;font-size: 14px;text-align: right;">
                                                     <apex:outputText value="{0, number, currency}">
                                                         <apex:param value="{!installoptionFee.Price}"/>
                                                     </apex:outputText>
                                                 </td>
                                                 <td style="width:105px;font-size: 14px;padding-bottom: 10px;text-align: right;">
                                                     <apex:outputPanel rendered="{!!installoptionAndFee.hasChildren}" layout="none">
                                                         <apex:outputText style="font-size: 14px;" value="{0, number, currency}">
                                                             <apex:param value="{!installoptionFee.Total}"/>
                                                         </apex:outputText>
                                                     </apex:outputPanel>
                                                 </td>
                                                 <td style="font-weight:bold;font-size: 14px;width:88px;padding-bottom: 10px;padding-right:20px;">
                                                     <apex:outputPanel rendered="{!index2 == unitDeal.InstalloptionAndFeeList.size && index3 == installoptionAndFee.OptionAndFeeList.size}" layout="none">
                                                        
                                                         <apex:outputText value="{0, number, currency}">
                                                             <apex:param value="{!unitDeal.InstalledOptionCost}"/>
                                                         </apex:outputText>
                                                     </apex:outputPanel>
                                                 </td>
                                            </tr>
                                        </apex:repeat>
                                    
                                </tbody>
                            </apex:repeat>
                        </table>
                     </apex:outputPanel>
                 </div>
                 <!-- END: Installed Options -->
                 <!-- START: Merchandise -->
                 <div style="width:600px;margin-left:50px;margin-top:15px;float:left;">
                     <apex:outputPanel rendered="{!unitDeal.MerchandiseOptionAndFeeList.size > 0}" layout="none">
                            <div>
                                <div style="border-bottom: 1px solid black;margin-left:19px;margin-bottom: 10px;font-size: 14px;">Merchandise
                                </div>
                                <table style="width:700px;">
                                    <apex:repeat value="{!unitDeal.MerchandiseOptionAndFeeList}" var="merchandiseOptionAndFee">
                                        <apex:variable var="index4" value="{!index4 + 1}" />
                                        <apex:variable var="index5" value="{!0}" />
                                        <tbody>
                                            <apex:outputPanel rendered="{!merchandiseOptionAndFee.hasChildren}" layout="none">
                                                <tr>
                                                    <td style="width:30px;padding-bottom: 10px;"></td>
                                                    <td style="width:120px;font-size: 14px;padding-bottom: 10px;">{!merchandiseOptionAndFee.ItemDescription}</td>
                                                    <td style="width:20px;padding-bottom: 10px;"></td>
                                                    <td style="width:40px;padding-bottom: 10px;"></td>
                                                    <td style="width:5px;padding-bottom: 10px;"></td>
                                                    <td style="width:5px;padding-bottom: 10px;"></td>
                                                    <td style="width:95px;font-size: 14px;padding-bottom: 10px;text-align: right;">
                                                        <apex:outputText value="{0, number, currency}">
                                                            <apex:param value="{!merchandiseOptionAndFee.Price}"/>
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="width:88px;padding-bottom: 10px;"></td>
                                                </tr>
                                            </apex:outputPanel>
                                            <apex:repeat value="{!merchandiseOptionAndFee.OptionAndFeeList}" var="merchandiseOptionFee">
                                                <apex:variable var="index5" value="{!index5 + 1}" />
                                                <tr>
                                                    <td style="width:30px;padding-bottom: 10px;"></td>
                                                    <td style="width:120px;font-size: 14px;padding-bottom: 10px;{!if(merchandiseOptionAndFee.hasChildren, 'padding-left:20px', 'padding-left:0px')};">{!merchandiseOptionFee.Item}</td>
                                                    <td style="width:37px;padding-bottom: 10px;"></td>
                                                    <td style="width:35px;font-size: 14px;padding-bottom: 10px;">
                                                        <apex:outputText value="{0, number, ###}">
                                                            <apex:param value="{!merchandiseOptionFee.Qty}" />
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="font-size: 14px;padding-bottom: 10px;">@</td>
                                                    <td style="font-size: 14px;padding-bottom: 10px;  text-align: right;">
                                                        <apex:outputText value="{0, number, currency}">
                                                            <apex:param value="{!merchandiseOptionFee.Price}"/>
