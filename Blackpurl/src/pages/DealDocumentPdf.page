        <apex:OutputPanel rendered="{!dealDocumentDetailRecord.IsSignatureLineforDealDocument}" layout="none">
            <apex:outputText style="margin-top:15px;float:left; font-size:14px;width: 90px;" value="Buyer :"  />
                <div style="border-bottom: 1px solid black;float:left;width: 220px;padding-top:30px;"></div>
         </apex:OutputPanel>
         <apex:OutputPanel rendered="{!dealDocumentDetailRecord.IsSignatureLineforDealDocument}" layout="none">
            <apex:outputText style="margin-top:15px;float:left; font-size:14px;width: 90px; padding-left:60px" value="Seller :"  />
                <div style="border-bottom: 1px solid black;float:left;width: 220px;padding-top:30px;"></div>
