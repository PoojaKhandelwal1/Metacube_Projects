<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0"
	controller="GlobalController">

    <!-- <apex:stylesheet value="{!Application}/css/LogServiceWorkPopWizard.css"/> -->
   
	<div>
	    <div id="logservice" class="modal fade" role="dialog" ng-cloak="ng-cloak">
	        <div class="modal-dialog logservice-dialog">
	            <!-- Modal content-->
	            <div class="modal-content col-md-12">
	                <div class="modal-body popup_container fee_popup_container">
	                    <div class="right_heading_container col-md-12 popupright ">
	                    <h1 class="right_panel_heading">{!$Label.COPage_Log_Technician_Time}</h1>
	                    <div class="savecancle_btncntnr">
	                    <button type="button" id="closemodalup"
	                            class="btn btn-default grayBtn"
	                            ng-attr-tabindex ="112" 
	                            ng-click="AddEditHourLog.closePopup1();">{!$Label.Cancel_Label}</button>
	                    <button type="button" class="btn btn-default greenBtn"
	                                ng-attr-tabindex ="107"
	                                ng-click="AddEditHourLog.saveHourLogData();"
	                                ng-disabled="AddEditHourLog.disableSaveButton">{!$Label.Save_Label}</button>
	                        
	                        
	                  </div>
	            </div>
	                            <div class="popupinside logservicepopup">
	                                <div class="col-md-12 InfoMT logservices">
	                                    <div class="col-md-2 logservicelabel "><p class="T3 col-md-10 ">{!$Label.Technician}</p><!-- {!$Label.Label_Employee} -->
	                                    </div>
	                                        <div class="col-md-9">
	                                            <div class="lightbox pull-left">
	                                                <div class="starimgdiv starimgdivlog  col-md-3 Plr0 MB12">
	                                                     <img class="Profile_pic" src="{!Application_Images}/profile_default.jpg" style="height:30px;width:100%"/>
	                                                     <!-- <div class="sprite profile_default"></div> -->
	                                                </div>
	                                                <div class="infotext">
	                                                    <p class="infoname">{{AddEditHourLog.HourLogModel.EmployeeName}}</p>
	                                                </div>
	                                                 <div class="itemiconlog itemicon" 
	                                                 ng-class="{'shrinkItemIcon' : AddEditHourLog.HourLogModel.EmployeeName == '' 
	                                                 || AddEditHourLog.HourLogModel.EmployeeName == null || AddEditHourLog.HourLogModel.EmployeeName == undefined}"> <!-- ng-class added by kajal 11th may, 2017 -->
	                                                        <p class="T7">{!$Label.Technician} | <span class="userStatus feeuserstatus ng-scope">{!$Label.Label_Active}</span></p><!-- {!$Label.Label_Employee} -->
	                                                    </div>
	                                            </div>
	                                        
	                                           <div class="pull-left TechnicianList" id="hoursLoggedPopupUserAutoComplete"><!-- class TechnicianList added by kajal 11th may, 2017 -->
	                                                  <button type="button" class="btn dropdown-toggle caretBtn" data-toggle="dropdown" 
	                                                        aria-haspopup="true"  ng-click = "AddEditHourLog.HourLogModel.OpenUser = !AddEditHourLog.HourLogModel.OpenUser; AddEditHourLog.showTechnicianList($event);">
	                                                        <span class="caret"></span>
	                                                        <span class="sr-only">Toggle Dropdown</span>
	                                                    </button>
	                                                    <!-- START: edited by kajal 11th may, 2017 -->
	                                                    <!-- <ul class="override-dropdown dropdown-menu  keep_open" Style = "width: 300px;"> 
	                                                    <usersearch id ="Owner_Search"   typesearchmodal="AddEditHourLog.HourLogModel.NewUser"  
	                                                         titleField="Name" placeholder ="Enter a Name"
	                                                         tabIndex = "110" rel="homePageSpecificPerson" />
	                                                        
	                                                    <li> </li>
	                                                    </ul>  -->
	                                                    <ul class="override-dropdown dropdown-menu keep_open">
	                                                        <li ng-repeat="technician in AddEditHourLog.TechnicianList"
	                                                            ng-click="AddEditHourLog.setCurrentTechnician(technician.Id)">
	                                                            <a><span>{{technician.TechinicianName}}</span></a>
	                                                        </li>
	                                                    </ul> 
	                                                    <!-- END: edited by kajal 11th may, 2017 -->
	                                                </div>
	                                        </div>
	                                    </div>
	                                    <div class="col-md-12 InfoMT"> 
	                                   <div class="col-md-2 logservicelabel"><p class="T3 col-md-10 ">{!$Label.Label_Date}</p>
	                                    </div>
	                                     <div class="col-md-9">
	                                        <div class="pull-left Plr0">
	                                            <p class="input-group" style="width: 200px;">
													<input class="form-control datepicker"
	                                            	ng-attr-tabindex ="101" id = "logServiceWizardDate"
	                                           		ng-class="{'redborder' : AddEditHourLog.hourLogFormValidationModal['StartDate']['isError'] == true}" 
	                                              	ng-model="AddEditHourLog.HourLogModel.StartDate"
	                                              	type="text" ui-date="dateOptions" ui-date-format="{{AddEditHourLog.dateFormat}}"
	                                              	style="background: #FFF;width: 200px;"
												    readonly="readonly" /><!-- Edited by kajal 01/30/2017 -->
												    <span class="input-group-addon btn" style="cursor:pointer; z-index: 99; position: absolute;right: 10%;top: 18%; padding: 0;font-size: 18px;">
								                    	<span class="glyphicon glyphicon-calendar" ng-click = "AddEditHourLog.showCalendar('logServiceWizardDate')"></span>
								                    </span><!--right: 9%; Added by kajal 01/30/2017 -->
	                                            </p>
	                                        </div>  
	                                     </div>
	                                 </div>
	                                 
	                                 <div class="col-md-12 InfoMT"> 
	                                   <div class="col-md-2 logservicelabel"><p class="T3 col-md-10">{!$Label.In}</p>
	                                    </div>
	                                     <div class="col-md-9">
	                                        <div class="pull-left Plr0">
	
	                                            <p class="input-group" style="width: 200px;">
	                                            <input class="form-control"
	                                             id = "TimeIn" ng-model = "AddEditHourLog.HourLogModel.TimeIn" 
	                                            	ng-attr-tabindex ="101"
	                                           		style="width: 200px;" type="text"  
	                                           		ng-blur="AddEditHourLog.validateTime('TimeIn')"/>
	
	                                            </p>
	                                        </div>  
	                                     </div>
	                                 </div>
	                                 
	                                 <div class="col-md-12 InfoMT"> 
	                                   <div class="col-md-2 logservicelabel"><p class="T3 col-md-10">{!$Label.Out}</p>
	                                    </div>
	                                     <div class="col-md-9">
	                                        <div class="pull-left Plr0">
	
	                                            <p class="input-group" style="width: 200px;">
	                                            <input class="form-control"
	                                             id = "TimeIn" ng-model = "AddEditHourLog.HourLogModel.TimeOut" 
	                                            	ng-attr-tabindex ="101"
	                                           		style="width: 200px;" type="text"  
	                                           		ng-blur="AddEditHourLog.validateTime('TimeOut')"/>
	
	                                            </p>
	                                        </div>  
	                                     </div>
	                                 </div>
	                                 
	                                 
	                                 <!-- <div class="col-md-12 InfoMT"> 
	                                   <div class="col-md-2 logservicelabel"><p class="T3 col-md-10">Hours Logged</p> 
	                                    </div>
	                                     <div class="col-md-9">
	                                        <div class="pull-left Plr0">
	
	                                            <p class="input-group" style="width: 200px;">
	                                            <input class="form-control"
	                                            ng-attr-tabindex ="101"
	                                            ng-model="AddEditHourLog.HourLogModel.TimeSpent_H" 
	                                           style="width: 200px;" type="text"  />
	
	                                            </p>
	                                        </div>  
	                                     </div>
	                                 </div> -->
	                                <div class="col-md-12 InfoMT">
	                                   <div class="col-md-2 logservicelabel"><p class="T3 col-md-10">{!$Label.Label_Notes}</p>
	                                    </div>
	                                     <div class="col-md-9">
	                                        <div class="pull-left Plr0">
	                                            <textarea rows="4" cols="60" style="resize:none;" 
	                                            ng-attr-tabindex ="108" 
	                                            ng-model="AddEditHourLog.HourLogModel.Notes"></textarea>
	                                        </div>  
	                                        <div>
	                                       
	                                        <p class="T3 M10" style="float: left;  width: auto; ">
	                                         <i class="fa fa-exclamation fa-2x " aria-hidden="true" style="color:red; margin-left: 15px;float: left;"></i>
	                                        <span class="chk_tick" style ="margin-left: 5px;"  ng-class="{'chked' : AddEditHourLog.HourLogModel.NotesRequired}"  ng-click = "AddEditHourLog.HourLogModel.NotesRequired = !AddEditHourLog.HourLogModel.NotesRequired " >
	                                          <i class="fa fa-check-square chksqure" ></i>
	                                        </span>
	                                        </p>
	                                     </div>
	                                     </div>
	                                 </div>
	                                <div class="right_heading_container col-md-12">
	                                <div class="savecancle_btncntnr pull-right">
	                                <button type="button" id="closemodal" ng-keydown="keydownevent()" 
	                                        ng-attr-tabindex ="110" 
	                                        class="btn btn-default grayBtn" ng-click="AddEditHourLog.closePopup1();">{!$Label.Cancel_Label}</button>
	                                    <button type="button" class="btn btn-default greenBtn" 
	                                    ng-attr-tabindex ="109" 
	                                    ng-click="AddEditHourLog.saveHourLogData();" 
	                                    ng-disabled="AddEditHourLog.disableSaveButton">{!$Label.Save_Label}</button>
	                                    
	                                    
	                                </div>
	                            </div>
	                    
	                    </div>
	                </div>
	            </div>
	        </div>
	    </div>
	  </div>
</apex:page>