<apex:page showHeader="false" renderAs="pdf" sidebar="false" standardstylesheets="false"  applyHtmlTag="false" docType="html-5.0" applyBodyTag="false" controller="PrintCODepositCtrl">
    <body style="font-family: Arial Unicode MS,sans-serif;font-size: 14px; margin-top:0px;">
        <div style="{!if(companyLogoURL != null, 'height:130px', 'height:70px')};width:800px;margin-bottom:30px;">
            <apex:outputPanel rendered="{!companyLogoURL != null}">
                <div style="width:135px;float: left;">
                    <img height="130px;" src="{!(companyLogoURL)}" width="130px;" style=" width: 100%;height: 100%; padding-top: 10px;"/>
                </div>
            </apex:outputPanel>
             <div style="width:390px;float: left;"> 
                 <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;text-transform: capitalize;" value="{!printCODepositRecord.StoreInfo.CompanyName}"></apex:outputText> <br/>
                 <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.Address1} {!printCODepositRecord.StoreInfo.Address2}"></apex:outputText><br/>
                 <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!printCODepositRecord.StoreInfo.City != null}" value="{!printCODepositRecord.StoreInfo.City}"></apex:outputText>&nbsp;&nbsp;
                 <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.State}"></apex:outputText>&nbsp;&nbsp;  <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value=" {!printCODepositRecord.StoreInfo.PostalCode}"></apex:outputText>
                 <br/>
                 <span style="font-size:14px;font-family: Arial Unicode MS,sans-serif;">Phone: </span>
                <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.BusinessPhone}"></apex:outputText><br/>
                <span style="font-size:14px;font-family: Arial Unicode MS,sans-serif;">Email: </span>
                <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.BusinessEmail}"></apex:outputText><br/>
                <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!printCODepositRecord.StoreInfo.TaxId != '' && printCODepositRecord.StoreInfo.Country != 'Australia'}">Tax ID:&nbsp;</apex:outputText>
                <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!printCODepositRecord.StoreInfo.TaxId != '' && printCODepositRecord.StoreInfo.Country == 'Australia'}">ABN:&nbsp;</apex:outputText>
                <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!printCODepositRecord.StoreInfo.TaxId != ''}" value="{!printCODepositRecord.StoreInfo.TaxId}"></apex:outputText>
                
                
                
                <!--  <div style="width:350px;">
                     <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.BusinessPhone}"></apex:outputText> &nbsp;&nbsp;
                     <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" value="{!printCODepositRecord.StoreInfo.BusinessEmail}"></apex:outputText><br/>
                 </div>
                 <apex:outputText style="font-size:14px;font-family: Arial Unicode MS,sans-serif;" rendered="{!printCODepositRecord.StoreInfo.TaxId != ''}" value="{!printCODepositRecord.StoreInfo.TaxId}"></apex:outputText> -->
            </div>
            
            <div style="width: 220px;{!if(companyLogoURL == null, 'padding-left:120px', 'padding-left:0px')};font-family: Arial Unicode MS,sans-serif;float: right;font-size: 14px;">
                <div style=" border-bottom: 1px solid black;width:226px;float:right;margin-right:50px;margin-bottom:10px;font-weight: bold;">CUSTOMER ORDER</div>
                <div style="float:right; margin-right:50px;width:220px;">
                    <div style="width:100px;font-weight: bold;float: left;margin-bottom:10px;">Order #</div>
                    <div style="width:120px;float: left;margin-bottom:10px;">{!printCODepositRecord.COHeaderInfo.Name}</div>
                </div>
                <div style="float:right;margin-right:50px;width:220px;margin-top: 0px;">
                    <div style="width:100px;font-weight: bold;float: left;margin-bottom:10px;">Date</div>
                    <div style="width:120px;float: left;margin-bottom:10px;">{!printCODepositRecord.COHeaderInfo.OrderedDate}</div>
                </div>
            </div>
        </div>
        
        <div style="width:800px;height:110px;{!if(companyLogoURL == null, 'padding-top:15px', 'padding-top:0px')};font-family: Arial Unicode MS,sans-serif;">
            <div style="width:540px;float: left;">
                <div style="width:320px;">
                    <div style="width:360px;font-size:14px;font-weight: bold;font-family: Arial Unicode MS,sans-serif;border-bottom: 1px solid black;">
                        <span>SOLD TO</span>
                    </div>
                    <apex:outputPanel style="font-size: 14px;" layout="none" > 
                            <div style="width:390px;float: left;font-family: Arial Unicode MS,sans-serif;margin-top:10px;margin-bottom:8px;">   
                                <div style="width:220px;"></div>
                                    <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.CustomerName != null}" layout="none">
                                        <apex:outputText style="font-size:14px;padding-top:10px;font-weight: bold;" value="{!printCODepositRecord.CustomerInfo.CustomerName}"></apex:outputText><br/>
                                        </apex:outputPanel>
                                       <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingStreet1 != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingStreet1}"></apex:outputText>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingStreet2 != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingStreet2} "></apex:outputText>
                                        </apex:outputPanel>
                                       
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingCity != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingCity} "></apex:outputText><br/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingState != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingState}"></apex:outputText><br/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingCountry != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingCountry}"></apex:outputText><br/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.BillingPostalCode  != null}" layout="none">
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.BillingPostalCode }"></apex:outputText><br/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.Phone != null}" layout="none">
                                            <span style="font-size:14px;font-weight: bold;">Phone:</span>
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.Phone}"></apex:outputText><br/>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!printCODepositRecord.CustomerInfo.Email != null}" layout="none">
                                            <span style="font-size:14px;font-weight: bold;">Email:</span>
                                            <apex:outputText style="font-size:14px;" value="{!printCODepositRecord.CustomerInfo.Email}"></apex:outputText><br/> 
                                    </apex:outputPanel>
                            </div>
                         </apex:outputPanel>
                </div>
            </div>
        </div>
        <div style="width:800px;padding-top:25px;">
            <apex:outputPanel rendered="{!printCODepositRecord.MerchandiseCOKHRecord.MerchandiseCOKHList.size > 0}">
                <div style="width:700px;border-bottom: 1px solid black;margin-top:10px;">
                    <div style="width:150px;font-weight: bold;display: inline-block;">MERCHANDISE</div>
                    <div style="width:545px;text-align:right;display: inline-block;">
                        <apex:outputText value="{0, number, currency}" >
                           <apex:param value="{!printCODepositRecord.MerchandiseCOKHRecord.Total}"/>
                        </apex:outputText>
                    </div>
                </div>
                <table style="width:700px;margin-top:10px;">
                    <thead>
                        <tr>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Type}</th>
                            <th style="text-align:left;padding-left:8px;width: 270px;">Item/Description</th>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Status}</th>
                            <th style="text-align:left;padding-left:8px;width: 50px;">Qty</th>
                            <th style="text-align:left;padding-left:8px;width: 20px;"></th>
                            <th style="text-align:left;padding-left:8px;width: 70px;">{!$Label.Label_Price}</th>
                            <th style="width: 80px;padding-left:8px;text-align:left;">{!$Label.Label_Total}</th>
                         </tr>
                    </thead>
                    
                    <tbody>
                        <apex:repeat value="{!printCODepositRecord.MerchandiseCOKHRecord.MerchandiseCOKHList}" var="COKHItem">
                            <apex:outputPanel rendered="{!COKHItem.id != null}" layout="none">
                                <tr>
                                        <td style= "padding:8px;width: 80px;">Kit</td>
                                        <td style="width:270px;padding-left: 8px;">{!COKHItem.ItemDescription}</td>
                                        <td style="width:80px;"></td>
                                        <td style="width:50px;"></td>
                                        <td style="width:20px;"></td>
                                        <td style="width:70px;"></td>
                                        <td style="width:80px;padding-left: 8px;text-align: left;">
                                            <apex:outputText value="{0, number, currency}" >
                                               <apex:param value="{!COKHItem.Price}"/>
                                            </apex:outputText>
                                        </td>
                                        
                                    </tr>
                            </apex:outputPanel>
                            
                            <apex:repeat value="{!COKHItem.COLIList}" var="COlitem">
                                <tr >
                                                    <!-- ng-class-even="'C2'" -->                                               
                                                    <td style="padding: 8px;width:80px; {!if(COKHItem.Id != null, 'padding-left:16px', '')};">
                                                        <apex:outputPanel rendered="{!COlitem.IsPart}">Part</apex:outputPanel>
                                                        <apex:outputPanel rendered="{!COlitem.IsFee}">Fee</apex:outputPanel>
                                                    </td> 
                                                    <td  style="width:270px;padding-left: 8px;">{!COlitem.Item}</td>
                                                     <td style="width:80px;padding-left: 8px;">
                                                        <apex:outputPanel rendered="{!(COlitem.IsPart && !COlitem.NonInventoryPart) || !COlitem.IsPart}">
                                                                <apex:outputText value="{!COlitem.Status}" >
                                                            </apex:outputText>
                                                            </apex:outputPanel>
                                                            <apex:outputPanel rendered="{!COlitem.IsPart && COlitem.NonInventoryPart}">
                                                                <apex:outputText value="{!IF(COlitem.Status == 'Invoiced' , 'Invoiced' , 'In Stock')}" >
                                                            </apex:outputText>
                                                            </apex:outputPanel>
                                                        </td>
                                                    <td style="padding-left: 8px;width:50px;">
                                                        <apex:outputText value="{0, number, ###}">
                                                            <apex:param value="{!COlitem.Qty}" />
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="width:20px;" > @</td>
                                                    <td style="padding-left: 8px;width:70px; ">
                                                        <apex:outputText value="{0, number, currency}" >
                                                            <apex:param value="{!COlitem.Price}"/>
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="padding-left: 8px;text-align: left;width:80px;">
                                                        <apex:outputPanel rendered="{!COKHItem.Id == null}" layout="none">
                                                            <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal == null}">
                                                                <apex:param value="{!COlitem.Qty * COlitem.Price}"/>
                                                            </apex:outputText>
                                                            <apex:outputText value="{0, number, currency}" rendered="{!COlitem.SubTotal != null}">
                                                                <apex:param value="{!COlitem.SubTotal}"/>
                                                            </apex:outputText>
                                                        </apex:outputPanel>
                                                    </td>
                                            </tr>
                            </apex:repeat>
                      </apex:repeat>
                    </tbody>
                    
                </table>
            </apex:outputPanel>
            
            <apex:outputPanel >
                <apex:repeat value="{!printCODepositRecord.SOHeadersList}" var="soHeaderVar">
                    <div style="width:700px;border-bottom: 1px solid black;margin-top: 25px;">
                        <div style="font-weight: bold;width:200px;display: inline-block;text-transform: uppercase;">{!soHeaderVar.SoHeaderName}</div>
                        <div style="width:495px;text-align:right;display: inline-block;">
                            <apex:outputText value="{0, number, currency}">
                               <apex:param value="{!soHeaderVar.Total}"/>
                            </apex:outputText>
                       </div>
                    </div>
                    <div style="width:700px;">
                    <apex:outputPanel rendered="{!soHeaderVar.Unit != null}">
                    <div style="width:90px;margin-top: 10px;display: inline-block;">
                    <label style="font-weight: bold;padding-left:8px;">UNIT:</label>
                    </div>
                    <div style="width:579px;display: inline-block;padding-left:20px;">
                        {!soHeaderVar.Unit}
                    </div>
                    </apex:outputPanel>
                    </div>
                    <div style="width:700px;font-size: 14px;font-family: Arial Unicode MS,sans-serif;margin-bottom:8px;">
                    <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size > 0}">
                         <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size == 1}" layout="none">
                            <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                            <label style="font-weight: bold;">{!$Label.Label_Concern}:  </label>
                            </div>
                            <div style="width:560px;display: inline-block;padding-left:32px;">
                                <span>{!soHeaderVar.ConcernList[0]}</span>
                            </div>
                         </apex:outputPanel>
                         <apex:outputPanel rendered="{!soHeaderVar.ConcernList.size > 1}" layout="none">
                            <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                <label style="font-weight: bold;">{!$Label.Label_Concern}:  </label>
                            </div>
                            <div style="width:599px;position: relative;margin-left: 48px;">
                                 <ul style="padding-left:40px;">
                                    <apex:repeat value="{!soHeaderVar.ConcernList}" var="concern">
                                         <li>{!concern}
                                        </li>
                                    </apex:repeat>
                                </ul>
                            </div>
                         </apex:outputPanel>
                    </apex:outputPanel>
                    </div>
                    <div style="width:700px;font-size: 14px;font-family: Arial Unicode MS,sans-serif;margin-bottom:8px;">
                    <apex:outputPanel rendered="{!soHeaderVar.CauseList.size > 0}">
                         <apex:outputPanel rendered="{!soHeaderVar.CauseList.size == 1}" layout="none">
                            <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                            <label style="font-weight: bold;">{!$Label.Label_Cause}:  </label>
                            </div>
                            <div style="width:560px;display: inline-block;padding-left:32px;">
                                <span>{!soHeaderVar.CauseList[0]}</span>
                            </div>
                         </apex:outputPanel>
                         <apex:outputPanel rendered="{!soHeaderVar.CauseList.size > 1}" layout="none">
                            <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                <label style="font-weight: bold;">{!$Label.Label_Cause}:  </label>
                            </div>
                            <div style="width:599px;position: relative;margin-left: 48px;">
                                 <ul style="padding-left:40px;">
                                    <apex:repeat value="{!soHeaderVar.CauseList}" var="cause">
                                         <li>{!cause}
                                        </li>
                                    </apex:repeat>
                                </ul>
                            </div>
                         </apex:outputPanel>
                    </apex:outputPanel>
                    </div>
                    <div style="width:700px;font-size: 14px;font-family: Arial Unicode MS,sans-serif;margin-bottom:8px;">
                    <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size > 0}">
                         <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size == 1}" layout="none">
                            <div style="width:72px;display: inline-block;margin-left:8px;margin-top: 10px;">
                            <label style="font-weight: bold;">{!$Label.Label_Correction}:  </label>
                            </div>
                            <div style="width:560px;display: inline-block;padding-left:32px;">
                                <span>{!soHeaderVar.CorrectionList[0]}</span>
                            </div>
                         </apex:outputPanel>
                         <apex:outputPanel rendered="{!soHeaderVar.CorrectionList.size > 1}" layout="none">
                            <div style="width:90px;position: absolute;margin-left:18px;margin-top: 10px;">
                                <label style="font-weight: bold;">{!$Label.Label_Correction}:  </label>
                            </div>
                            <div style="width:599px;position: relative;margin-left: 48px;">
                                 <ul style="padding-left:40px;">
                                    <apex:repeat value="{!soHeaderVar.CorrectionList}" var="correction">
                                         <li>{!correction}
                                        </li>
                                    </apex:repeat>
                                </ul>
                            </div>
                         </apex:outputPanel>
                    </apex:outputPanel>
                    </div>
                    <table style="width:700px; margin-top:10px;">
                    <thead>
                        <tr>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Type}</th>
                            <th style="text-align:left;padding-left:16px;width: 270px;">Item/Description</th>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Status}</th>
                            <th style="text-align:left;padding-left:8px;width: 50px;">Qty</th>
                            <th style="text-align:left;padding-left:8px;width: 20px;"></th>
                            <th style="text-align:left;padding-left:8px;width: 70px;">{!$Label.Label_Price}</th>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Total}</th>
                         </tr>
                    </thead>
                    
                    <tbody>
                        <apex:repeat value="{!soHeaderVar.SOGridItems}" var="soGrid">
                            <apex:outputPanel rendered="{!soGrid.SOKH.id != null}" layout="none">
                                <tr>
                                        <td style= "padding:8px;width: 80px;">Kit</td>
                                        <td style="width: 270px;padding-left:16px;">{!soGrid.SOKH.ItemDescription}</td>
                                        <td style="width: 80px;"></td>
                                        <td style="width: 50px;"></td>
                                        <td style="width: 20px;"></td>
                                        <td style="width: 70px;"></td>
                                        <td style="text-align: left ;padding-left:8px;width: 80px;">
                                            <apex:outputText value="{0, number, currency}" >
                                               <apex:param value="{!soGrid.SOKH.Price}"/>
                                            </apex:outputText>
                                        </td>
                                        
                                    </tr>
                            </apex:outputPanel>
                            
                            <apex:repeat value="{!soGrid.SOKH.SOLIList}" var="SoLi">
                                <tr >
                                                    <!-- ng-class-even="'C2'" -->                                               
                                                    <td style="padding: 8px;width: 80px; {!if(soGrid.SOKH.Id != null, 'padding-left:16px', '')};">
                                                        <apex:outputPanel rendered="{!SoLi.IsPart}">Part</apex:outputPanel>
                                                        <apex:outputPanel rendered="{!SoLi.IsFee}">Fee</apex:outputPanel>
                                                        <apex:outputPanel rendered="{!SoLi.IsLabour}">Labor</apex:outputPanel>
                                                        <apex:outputPanel rendered="{!SoLi.IsSublet}">Sublet</apex:outputPanel>
                                                    </td> 
                                                    <td  style="width:270px;padding-left:16px;">{!SoLi.ItemDesc}</td>
                                                  <td style="width:80px;padding-left:8px;">
                                                    <apex:outputPanel rendered="{!SoLi.IsPart && !SoLi.IsNonInventoryPart}">
                                                        <apex:outputText value="{!SoLi.Status}" >
                                                        </apex:outputText>
                                                    </apex:outputPanel>
                                                    <apex:outputPanel rendered="{!SoLi.IsPart && SoLi.IsNonInventoryPart}">
                                                        <apex:outputText value="{!IF(SoLi.Status == 'Invoiced' , 'Invoiced' , 'In Stock')}" >
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                                <apex:outputPanel rendered="{!SoLi.IsFee || SoLi.IsLabour || SoLi.IsSublet}">
                                                    <apex:outputText value="{!IF(soHeaderVar.SoHeaderInvoiceStatus == 'Invoiced' , 'Invoiced' , 'Ready')}" >
                                                    </apex:outputText>
                                                </apex:outputPanel>
                                            </td>
                                                    <td style="text-align: left;width:50px;padding-left:8px;">
                                                        <apex:outputText value="{0, number, ###}">
                                                            <apex:param value="{!SoLi.QtyNeeded}" />
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="width:20px;"> @</td>
                                                    <td style="text-align:left;width:70px;padding-left:8px; ">
                                                        <apex:outputText value="{0, number, currency}" >
                                                            <apex:param value="{!SoLi.KitPrice}"/>
                                                        </apex:outputText>
                                                    </td>
                                                    <td style="text-align:left;width:80px;padding-left:8px;">
                                                        <apex:outputPanel rendered="{!soGrid.SOKH.Id == null}" layout="none">
                                                            <apex:outputText value="{0, number, currency}">
                                                                <apex:param value="{!SoLi.QtyNeeded * SoLi.KitPrice}"/>
                                                            </apex:outputText>
                                                        </apex:outputPanel>
                                                    </td>
                                            </tr>
                            </apex:repeat>
                      </apex:repeat>
                    </tbody>
                    
                </table>
                </apex:repeat>
        </apex:outputPanel>
        
        <apex:outputPanel rendered="{!printCODepositRecord.DealInfo.UnitList.size > 0}">
                <div style="width:700px;border-bottom: 1px solid black;margin-top:20px;  float: left;">
                    <div style="width:150px;font-weight: bold;display: inline-block;  float: left;">DEAL</div>
                    <div style="width:545px;text-align:right;display: inline-block; float: left;">
                        <apex:outputText value="{0, number, currency}" >
                           <apex:param value="{!printCODepositRecord.DealInfo.DealTotal}"/>
                        </apex:outputText>
                    </div>
                </div>
                <div style="float: left;">
                           <div style="float: left;">
                              <apex:outputpanel rendered="{!printCODepositRecord.DealInfo.DealFinanceObj.CoBuyerId != null}" layout="none">
                              <span>Co-buyer: </span>{!printCODepositRecord.DealInfo.DealFinanceObj.CoBuyerName}
                              </apex:outputpanel>
                           </div>
                        </div>
              <div style="float: left;">
                
                
                
                <table style="width:700px;margin-top:10px;">
                    <thead>
                        <tr>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Type}</th>
                            <th style="text-align:left;padding-left:8px;width: 270px;">Item/Description</th>
                            <th style="text-align:left;padding-left:8px;width: 80px;">{!$Label.Label_Status}</th>
                            <th style="text-align:left;padding-left:8px;width: 50px;">Qty</th>
                            <th style="text-align:left;padding-left:8px;width: 20px;"></th>
                            <th style="text-align:left;padding-left:8px;width: 70px;">{!$Label.Label_Price}</th>
                            <th style="width: 80px;padding-left:8px;text-align:left;">{!$Label.Label_Total}</th>
                         </tr>
