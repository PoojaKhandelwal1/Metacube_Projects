	                                                    class="parentBody" ng-init="DuplicateSectionIndex = $index"> <!-- by kv 01/26/2017 -->
	                                                        <td width="33%">{{DuplicateSection.SectionName}}</td>
	                                                        <td width="10%" class="text-center">{{DuplicateSection.Qty}}</td>
	                                                        <td width="57%"></td>
	                                                    </tr>
	                                                </tbody>
	                                        </table>
