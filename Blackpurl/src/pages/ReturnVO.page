                        <span class="T3 pull-right total">
                        	<p id="vendor_order_total">
                            	{{RVO.Data.SubTotal | currency}}
                          	</p>
                        </span>
                        
                        <!-- END: added by kajal 30th march 2017 #1760 -->
                        <span class="T3 pull-right total">
                        	<p id="vendor_order_total">
                            	{{RVO.GroupTotalCost | currency}}
                            </p>
                        </span> 
                    </div>
                </div>
                <div ng-show="RVO.showFinalizeSection()">
