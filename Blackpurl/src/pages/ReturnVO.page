                                            <label class="col-md-3 T3 Plr0">{!$Label.Label_Status}</label>
                                            <span class="col-md-7 Plr0 FC10 T3"
                                            	ng-class="{'FCRed' : RVO.Data.Status == 'Credited', 'FCOrange' : RVO.Data.Status == 'Submitted' || RVO.Data.Status == 'Approved'}">
                                            	{{RVO.Data.Status}}
                                            </span>
                                         </div>
