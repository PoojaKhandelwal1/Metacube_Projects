<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0" controller="GlobalController">
		<!-- <apex:stylesheet value="{!Application}/css/PrintServiceWorkSheetPopUp.css"/>  -->
         <div id="PrintServiceWorksheetPopUp" class="modal fade PrintServiceWorksheetPopUp" role="dialog">
        <div class="modal-dialog modal-dialog_printSO" ng-class="{'edit_modal-dialog' : KitCompModal.isEditMode == true}">
            <!-- Modal content-->
            <div class="modal-content col-md-12">
                <div class="modal-body popup_container__printSO">
                    <div class="popupinside">
                        <div>
                        	<p class="printSO_Heading">Print Service Worksheets for:</p>
                        </div>
                        <div class="SORecordsContainer">
	                        <div class="form_fields SOUnitRecordContainer" ng-repeat="unit in PrintServiceWorksheetServicePopUp.PrintServiceWorksheetServiceModel">
	                            <div class="form-group large" ng-click="PrintServiceWorksheetServicePopUp.updateCheckBoxForUnit($index);">
	                                <label class="checkbox smschkbox"> 
	                                    <span class="chk_tick" 
	                                    	ng-attr-tabindex="" 
	                                    	ng-enter=""
	                                    	ng-class="{'chked': unit.IsUnitSelected}" id="unit{{$index}}" > 
	                                        <i class="fa fa-check-square chksqure"></i>
	
	                                    </span>
	                                    <p class="checkBoxText" ng-if="unit.VINNumber != null">Unit: {{unit.VINNumber}}</p>
	                                    <p class="checkBoxText" ng-if="unit.VINNumber == null">Unit: <span class="vinUnknown">VIN unknown</span></p>
	                                </label>
	                            </div>
	                            <div class="form_fields SORecordForUnitContainer" ng-repeat="so in unit.SOInfoWrapperList">
		                            <div class="form-group large" ng-click="PrintServiceWorksheetServicePopUp.updateCheckBoxForEachSO($index, $parent.$index);">
		                                <label class="checkbox smschkbox"> 
		                                    <span class="chk_tick" 
		                                    	ng-attr-tabindex=""  
		                                    	ng-enter=""
		                                    	ng-class="{'chked': so.IsSOHeaderSelected}" id="unit{{$index}}" > 
		                                        <i class="fa fa-check-square chksqure"></i>
		
		                                    </span>
		                                    <p class="checkBoxText">{{so.Name}}</p>
		                                </label>
		                            </div>
		                        </div>
	                        </div>
	                    </div>
                        <div>
                        	<button ng-click="PrintServiceWorksheetServicePopUp.closePopup();" class="btn  grayBtn pull-right">Cancel</button>
                        	<button ng-click="PrintServiceWorksheetServicePopUp.printServiceWorkSheet(PrintServiceWorksheetServicePopUp.COHeaderId, true);" 
                        		ng-disabled="PrintServiceWorksheetServicePopUp.disablePrintButton();" class="btn  blueBtnPrintReview pull-right ">Print Review</button>
							<button ng-click="PrintServiceWorksheetServicePopUp.printServiceWorkSheet(PrintServiceWorksheetServicePopUp.COHeaderId, false);" 
								ng-disabled="PrintServiceWorksheetServicePopUp.disablePrintButton();" class="btn  blueBtn pull-right ">Print Worksheet</button>
						</div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</apex:page>