<apex:page standardStylesheets="false" showChat="false" showHeader="false" sidebar="false" docType="html-5.0"  controller="GlobalController">
            <!-- <apex:stylesheet value="{!Application}/css/AccountingIntegrationSettings.css"/> -->
             
            <div>
            <route-loading-indicator></route-loading-indicator>
            <div class="fadein" ui-view="AddEditCategory" autoscroll='false'></div>
            <div ng-cloak="ng-cloak" id="AccountingSetupController"> <!-- ng-if="!isRouteLoading"  -->
            <div class="content cashSaleContainer"> <!-- ng-show="CashSaleModel.showContent" -->
                <!-- Start: cash Sale Crumbs Section -->
                <div class="row cashSaleCrumbsDiv M0">
                    <div id="cashSaleCrumbs" class="col-xs-12 col-sm-12 col-md-12 col-lg-12 cashSaleCrumbs p0">
                       <ul class="col-xs-12 col-sm-12 col-md-12 col-lg-12 p0 cashSaleCrumbs_UL">
                          <li class="col-xs-1 col-sm-1 col-md-1 col-lg-1 p0 cashSaleCrumbs_li1"
                          ng-class="{'tax-code-include-li': AccountingSetupModel.accountingData.configuration.IsTaxCodesEnabled }"
                           ng-repeat = "accRec in AccountingSetupModel.AccountingSetupStepsList">
                             <a ng-if = "$index == 0" class="M0 backBtn" ng-class="{'previousElementOfactiveChevron' : AccountingSetupModel.selectedAccountingChevronIndex == '1', 
                                'activeChevron' : AccountingSetupModel.selectedAccountingChevronIndex == '0'}" ng-click="AccountingSetupModel.NextActionOnBack()">
                                <i class="fa fa-angle-left" aria-hidden="true" style="font-size: 20px;"></i> Back
                             </a>  <!--  --> <!-- href="{!$Page.UserSettings}" -> : Pooja 8 Mar. 2017 --> <!-- class: backBtn: Pooja 9 Mar. 2017 -->
                                <a ng-if = "$index != 0" class="M0" ng-class="{'previousElementOfactiveChevron' : AccountingSetupModel.selectedAccountingChevronIndex == $index + 2, 
                                'activeChevron' : AccountingSetupModel.selectedAccountingChevronIndex == $index}"
                                ng-click="AccountingSetupModel.NextAction($index)">{{accRec.Name}}</a>
                          </li>
                       </ul>
                    </div>
                </div>
                <!-- End: cash Sale Crumbs Section -->
                
                <!-- Start: cash Sale Content Section -->
                <div class="row AccountingContent M0">
                    <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8 leftPanel">
                        <div class="AccountingProvider" ng-if="AccountingSetupModel.displaySection == 'Accounting Provider'"><!-- ng-if -->
                            <div class="HeaderText">
                                <h3>1. Accounting Provider</h3>
                            </div>
                            <div class="input-group AcoountingDropDown-input-group">
                                <div class="input-group-btn">
                                    <button class="btn btn-default dropdown-toggle AcoountingDropDown-dropdown-toggle" data-toggle="dropdown" 
                                    ng-class="{'inputTextColor': AccountingSetupModel.selectedAccountingProvider != 'Select One'}"
                                    type="button" aria-haspopup="true" aria-expanded="false">
                                        <span class="fa fa-angle-down"></span>
                                        {{AccountingSetupModel.selectedAccountingProvider}}<!-- {{AccountingSetupModel.accountingData.configuration.AccountingProvider}} -->
                                    </button> 
                                    <ul class="dropdown-menu AccountingUL" role="menu">
                                        <li data-value="AccountingProvider.Name" ng-repeat="AccountingProvider in AccountingSetupModel.AccountingProvidersList">
                                            <a  ng-click="AccountingSetupModel.ProviderSelected(AccountingProvider.Name);">
                                                <span>{{AccountingProvider.Name}}</span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="ConnectToQBButton" ng-if="AccountingSetupModel.selectedAccountingProvider == 'Quickbooks Online' && !AccountingSetupModel.accountingData.configuration.IsConnected">
                                 <img class="" src="{!Application_Images}/C2QB_green_btn_med_default.png"
                                    onmouseover="this.src='{!Application_Images}/C2QB_green_btn_med_hover.png'"
                                    onmouseout="this.src='{!Application_Images}/C2QB_green_btn_med_default.png'"
                                    ng-click="AccountingSetupModel.showGetIntegrationDataPopup(AccountingSetupModel.selectedAccountingProvider)"/> 
                                 <!-- <div class="sprite"
                                 ng-click="AccountingSetupModel.showGetQBDataPopup()"></div>  -->
                            </div> 
                            <div class="cnnect-integration-button" ng-if="AccountingSetupModel.selectedAccountingProvider == 'Xero' && !AccountingSetupModel.accountingData.configuration.IsConnected">
                                <i ng-click = "AccountingSetupModel.showGetIntegrationDataPopup(AccountingSetupModel.selectedAccountingProvider)" ng-include="'{!Application_Images}/Icons/connect-blue.svg'"></i>
                                
                                <!-- <button ng-click = "AccountingSetupModel.showGetIntegrationDataPopup(AccountingSetupModel.selectedAccountingProvider)">
                                    Connect To Integration
                                </button> -->
                            </div>
                            <div class="QBDetailsContainer" ng-if="AccountingSetupModel.selectedIntegrationName">
                                 <div class="QBConnectionSuccess" ng-if="AccountingSetupModel.accountingData.configuration.IsConnected">
                                    <p class="M0 QBConnectionDetailsLabel">
                                        Connected to {{AccountingSetupModel.selectedIntegrationName}} Company :
                                    </p>
                                    <p class="M0 QBConnectionDetailsValue">
                                        {{AccountingSetupModel.accountingData.configuration.CompanyName}}
                                    </p>
                                 </div>
                                 <div class="QBConnectionFailure" 
                                        ng-if="!AccountingSetupModel.accountingData.configuration.IsConnected && 
                                                AccountingSetupModel.accountingData.configuration.OAuthTokenSecret != null">
                                    <p class="M0 QBConnectionDetailsLabel">
                                        Error Connecting to Quickbooks
                                    </p>
                                    <p class="M0 QBConnectionDetailsValue">
                                    </p>
                                 </div>
                                 <div class="ActionButton TestConnectionBtn" 
                                    ng-if="AccountingSetupModel.accountingData.configuration.IsConnected">
                                    <button class="btn btn-default col-md-4 col-lg-4 AccountingIntegrationSetupButton" ng-class="" 
                                    ng-click="AccountingSetupModel.selectedAccountingProvider == 'Quickbooks Online' ? AccountingSetupModel.getQBCompanyInfo() : AccountingSetupModel.getConnectedCompanyInfo()" type="button"> 
                                        TEST CONNECTION
                                    </button> <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                                 </div>
                                 <!-- <div class="RemoveQBConnection" ng-if="AccountingSetupModel.selectedAccountingProvider == 'Quickbooks Online' && AccountingSetupModel.accountingData.configuration.IsConnected"> 
                                    <img src="{!Application_Images}/Cancel_Red.png"
                                    ng-click="AccountingSetupModel.RemoveQBConnection();"/>
                                    <span class="popupCancelAction" ng-click="AccountingSetupModel.RemoveQBConnection();">REMOVE QUICKBOOKS LINK</span> class="popupCancelAction": Added By Pooja 9 Mar. 2017
                                 </div> -->
                                 <div class="RemoveQBConnection" ng-if="AccountingSetupModel.accountingData.configuration.IsConnected"> 
                                    <img src="{!Application_Images}/Cancel_Red.png"
                                    ng-click="AccountingSetupModel.RemoveConnection();"/>
                                    <span class="popupCancelAction text-uppercase" ng-click="AccountingSetupModel.RemoveConnection();">REMOVE {{AccountingSetupModel.selectedIntegrationName}} LINK</span> <!-- class="popupCancelAction": Added By Pooja 9 Mar. 2017 -->
                                 </div>
                            </div>
                            <div class="ActionButton AccountingProviderNextAction">
                                <button ng-class="{'ActionBtnDisabled': (AccountingSetupModel.selectedAccountingProvider == 'Quickbooks Online' 
                                                    && !AccountingSetupModel.accountingData.configuration.IsConnected) ||
                                                        AccountingSetupModel.selectedAccountingProvider == 'Select One'}"
                                    ng-disabled="(AccountingSetupModel.selectedIntegrationName
                                                    && !AccountingSetupModel.accountingData.configuration.IsConnected) || 
                                                        AccountingSetupModel.selectedAccountingProvider == 'Select One'" class="btn btn-default col-md-4 col-lg-4 pull-right AccountingIntegrationSetupButton" 
                                                        ng-click="AccountingSetupModel.saveAccountingProviderName(AccountingSetupModel.selectedAccountingProvider);" type="button">
                                    NEXT
                                </button>  <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                            </div>
                        </div>
                        <div class="ChartOfAccounts" ng-if="AccountingSetupModel.displaySection == 'Chart of Accounts'"><!-- ng-if -->
                            <div class="HeaderText">
                                <h3>2. Chart of Accounts</h3>
                            </div>
                            <div class="SyncAccountsContainer" ng-if="AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Quickbooks Online' || AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Xero'">
                                <div class="col-md-4 col-lg-4 BtnContainer">
                                    <div class="ActionButton">
                                        <button class="btn btn-default col-md-4 col-lg-4 AccountingIntegrationSetupButton" ng-class="" ng-click="AccountingSetupModel.syncAccounts();" 
                                        ng-disabled="AccountingSetupModel.disableSyncNowBtn" type="button">
                                            SYNC NOW
                                        </button> <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                                    </div>
                                </div>
                                <div class="col-md-8 col-lg-8 DetailContainer">
                                    <p class="DetailLabel">Last Synced:</p>
                                    <p class="DetailValue">{{AccountingSetupModel.LastSyncTime}}</p>
                                    <!-- <p class="DetailValue">8:40 AM PST</p> -->
                                </div>
                            </div>
                            <div class="UploadAccountsContainer" ng-if="AccountingSetupModel.accountingData.configuration.AccountingProvider != 'Quickbooks Online' && AccountingSetupModel.accountingData.configuration.AccountingProvider != 'Xero'">
                                <div class="UploadAccountsTextContainer">
                                    <p class="DetailLabel">We must align the GL accounts used in your Accounting Solution 
                                    with the transactions processed within Blackpurl</p>
                                </div>
                                <p class="DetailLabel">
                                    Alignment method
                                </p>
                                <div class="input-group AcoountingDropDown-input-group">
                                    <div class="input-group-btn">

                                        <button class="btn btn-default dropdown-toggle AcoountingDropDown-dropdown-toggle" 
                                        ng-class="{'inputTextColor': AccountingSetupModel.AccountsUploadSelectedMethod != 'Select One'}"
                                        data-toggle="dropdown" type="button" aria-haspopup="true" aria-expanded="false">
                                            <span class="fa fa-angle-down"></span>
                                           {{AccountingSetupModel.AccountsUploadSelectedMethod}}<!-- User supplied list -->
                                        </button>
                                        <ul class="dropdown-menu AccountingUL" role="menu">
                                            <li data-value="UserSupplied">

                                                <a  ng-click="AccountingSetupModel.AccountsUploadMethod('User supplied list');">
                                                    <span>User supplied list</span>
                                                </a>
                                            </li>
                                            <li data-value="None">

                                                <a  ng-click="AccountingSetupModel.AccountsUploadMethod('None');">
                                                    <span>None</span>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="QBConnectionSuccess" 
                                ng-if="AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list' && AccountingSetupModel.accountingData.accountingList.length > 0">
                                    <p class="M0 QBConnectionDetailsLabel">
                                        Uploaded on :
                                    </p>
                                    <p class="M0 QBConnectionDetailsValue">
                                        {{AccountingSetupModel.LastSyncTime}} <!-- February 12, 2017 8:40 AM PST -->
                                    </p>
                                </div>
                                <div class="ActionButton" ng-if="AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list'">

                                    <button ng-if = "AccountingSetupModel.accountingData.accountingList.length == 0" class="btn btn-default col-md-4 col-lg-4 AccountingIntegrationSetupButton" 
                                   
                                    ng-click="AccountingSetupModel.UploadChartOfAccount()"  type="button"><!--  ng-class="{'ActionBtnDisabled': AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list'}"ng-disabled="AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list'" -->

                                        UPLOAD CHART OF ACCOUNTS
                                    </button> 
                                     <button class="btn btn-default col-md-4 col-lg-4 AccountingIntegrationSetupButton" ng-if = "AccountingSetupModel.accountingData.accountingList.length > 0"
                                   
                                    ng-click="AccountingSetupModel.showAccountsListPopup()"  type="button"><!--  ng-class="{'ActionBtnDisabled': AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list'}"ng-disabled="AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list'" -->

                                        VIEW CHART OF ACCOUNTS    
                                    </button>
                                    <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                                </div>
                                <div class="RemoveQBConnection"
                                ng-if="AccountingSetupModel.AccountsUploadSelectedMethod == 'User supplied list' && AccountingSetupModel.accountingData.accountingList.length > 0">  
                                    <img src="{!Application_Images}/Cancel_Red.png"
                                    ng-click="AccountingSetupModel.RemoveChartOfAccounts();"/>
                                    <!-- <div class="sprite Cancel_Red pull-left" ng-click="AccountingSetupModel.RemoveChartOfAccounts();"></div> -->

                                    <span class="popupCancelAction" ng-click="AccountingSetupModel.RemoveChartOfAccounts();">REMOVE CHART OF ACCOUNTS</span> <!-- class="popupCancelAction": Added By Pooja 9 Mar. 2017 -->
                                </div>   
                            </div>
                            <div class="ActionButton ChartsAccountNextAction"> 

                                <button class="btn btn-default col-md-4 col-lg-4 LargeBtn AccountingIntegrationSetupButton" ng-class="" ng-click="AccountingSetupModel.showAccountsListPopup();" 
                                ng-disabled="" type="button" 
                                ng-if="(AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Quickbooks Online' || AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Xero')
                                    && AccountingSetupModel.accountingData.accountingList.length > 0">
                                     VIEW CHART OF ACCOUNTS
                                 </button> <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                            </div>
                            <div class="RemoveQBConnection"
                                ng-if="(AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Quickbooks Online' || AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Xero')
                                    && AccountingSetupModel.accountingData.accountingList.length > 0">  
                                <img src="{!Application_Images}/Cancel_Red.png"
                                ng-click="AccountingSetupModel.RemoveChartOfAccounts();"/>
                                <!-- <div class="sprite Cancel_Red pull-left" ng-click="AccountingSetupModel.RemoveChartOfAccounts();"></div> -->
                                <span class="popupCancelAction" ng-click="AccountingSetupModel.RemoveChartOfAccounts();">REMOVE CHART OF ACCOUNTS</span> <!-- class="popupCancelAction": Added By Pooja 9 Mar. 2017 -->
                            </div>
                            <div class="ActionButton ChartsAccountNextAction ChartsAccountMarginBottom"> 

                                <button class="btn btn-default col-md-4 col-lg-4 pull-right AccountingIntegrationSetupButton" 
                                ng-class="{'ActionBtnDisabled': AccountingSetupModel.disableChartOfAccountsNextBtn()}" 
                                ng-click="AccountingSetupModel.saveAccountingAlignmentMethod(AccountingSetupModel.AccountsUploadSelectedMethod);" 
                                ng-disabled="AccountingSetupModel.disableChartOfAccountsNextBtn();" 
                                type="button">
                                    NEXT 
                                </button> <!-- class: AccountingIntegrationSetupButton: Added By Pooja 10 Mar. 2017 -->
                            </div>
                        </div>
                       <div class="ChartOfAccounts ControlAccounts tax-code-mapping-wrapper" ng-show="AccountingSetupModel.displaySection == 'Tax Code Mapping'"><!-- ng-if -->
                            <div class="HeaderText">
                                <h3>3. Tax Code Mapping</h3>
                            </div>
                            <div class="SyncAccountsContainer tax-code-mapping-container" ng-if="AccountingSetupModel.selectedIntegrationName">
                                <div class="BtnContainer no-border">
                                    <div>
                                        <button class="bp-btn bp-btn-normal H200" ng-click="AccountingSetupModel.syncTaxes();" 
                                        ng-disabled="AccountingSetupModel.disableSyncNowBtn" type="button">
                                            SYNC TAX CODES
                                        </button> 
                                    </div>
                                </div>
                                <div class="col-md-8 col-lg-8 DetailContainer">
                                    <p class="DetailLabel M0">Last Synced:</p>
                                    <p class="DetailValue M0">{{AccountingSetupModel.lastTaxCodeSync}}</p>
                                    <!-- <p class="DetailValue">8:40 AM PST</p> -->
                                </div>
                            </div>
                            <div class="ControlAccountsHeaderText" ng-if = "AccountingSetupModel.accountingData.configuration.AccountingProvider == 'Quickbooks Online'">
                                <p class="DetailLabel">Map QuickBooks Online tax rates to each of your Blackpurl tax rates</p>
                            </div>
                            
                            <div class = "ControlAccountsContainer tax-mapping-control-contener">
                                <section ng-if = "AccountingSetupModel.selectedIntegrationName == 'Quickbooks Online'">
                                <div class = "label-container">
                                    <p class="label-container-first">
                                        BLACKPURL TAX RATE
                                    </p>
                                    <p class="label-container-second">
                                        QUICKBOOKS ONLINE TAX RATE
                                    </p>
                                </div>
                                <div ng-repeat = "salesTaxItem in AccountingSetupModel.SalesTaxItemList" ng-init="salesTaxListItemIndex = $index">
                               <p class="DetailLabel">
                                   {{AccountingSetupModel.getTaxCodeMappingLabel(salesTaxItem.Name,salesTaxItem.TaxRate)}}
                                </p>
                                <div class="input-group AcoountingDropDown-input-group">
                                    <div class="input-group-btn">
                                        <button class="btn btn-default dropdown-toggle AcoountingDropDown-dropdown-toggle dropDownBtnPB dropDownWidth" 
                                            ng-class="{'inputTextColor': AccountingSetupModel.getTaxCodeMappingNameFromKey(salesTaxListItemIndex,true) != 'Select a GL Account'}"
                                            data-toggle="dropdown" type="button" aria-haspopup="true" aria-expanded="false"
                                            ng-if="AccountingSetupModel.taxRateList.length > 0"
                                            ng-click="AccountingSetupModel.addOpenClass(salesTaxItem.Name);"> 
                                                <span class="fa fa-angle-down dropDownArrorBtnMT"></span>
                                                <p class="tax-code-input"> <!-- 24 march -->
                                                    {{AccountingSetupModel.getTaxCodeMappingNameFromKey(salesTaxListItemIndex,true)}}<!-- ng-bind="AccountingSetupModel.getAccountNameFromKey('UndepositedFunds')" -->
                                                </p>
                                        </button> 
                                        <ul class="dropdown-menu AccountingUL keep_open dropDownWidth" role="menu" id="{{AccountingSetupModel.generateId(salesTaxItem.Name,'UL')}}">
                                            <div class="searchContainer">
                                                <i class="glyphicon glyphicon-search"></i> 
                                                <input class="form-control" id="{{AccountingSetupModel.generateId(salesTaxItem.Name,'Input')}}"
                                                ng-model="salesTaxItem.taxRateName" 
                                                placeholder="Search" type="search"/>
                                            </div>
                                            <div class="listItemsContainer">
	                                            <ul class = "taxCodeRecWrapper">
                                                <li data-value="" 
                                                    ng-repeat="taxRateRec in AccountingSetupModel.taxRateList | filter:salesTaxItem.taxRateName | orderBy:'AccountingId'">
                                                    <a  ng-click="AccountingSetupModel.SelectTaxRec($event,salesTaxListItemIndex,taxRateRec,true,salesTaxItem.Name);"
                                                       data-toggle="tooltip" title="{{taxRateRec.Name}} {{taxRateRec.RateValue}} %"     >
                                                        <span>{{taxRateRec.Name}} {{taxRateRec.RateValue}} %</span>
                                                    </a>
	                                                </li>
	                                             </ul>
                                            </div>
                                        </ul>
                                        <input type="text" class="form-control AcoountingDropDown-dropdown-toggle MT0 inputText" id="{{AccountingSetupModel.generateId(salesTaxItem.Name,'')}}" placeholder="Select a GL Account" maxlength="50"
                                            ng-model="salesTaxItem.taxRateName" rel="{{AccountingSetupModel.generateId(salesTaxItem.Name,'Id')}}"
                                            data-customtoggle="tooltip"
                                            ng-if="AccountingSetupModel.taxRateList.length == 0"
                                            ng-change="AccountingSetupModel.isAccountingSetupDataSaved = false;"/> 
                                        <span class="HelperText BlueTooltip tax-code-mapping-tooltip" id="{{AccountingSetupModel.generateId(salesTaxItem.Name,'Tooltip')}}">
                                            <i class="tax-code-warining red_EB5757 bp-cursor-pointer " ng-include="'{!Application_Images}/Icons/warning-filled.svg'"
                                            data-customtoggle="tooltip"  data-original-title="{{salesTaxItem.ErrorMessage}}"></i>
                                        </span>
                                    </div>
                                </div>
                                </div>
                                	</section>
                                <section>
                                <div class="ControlAccountsHeaderText">
                                    <p class="DetailLabel">Map {{AccountingSetupModel.selectedIntegrationName}} tax codes to each of your Blackpurl tax codes</p>
                                </div>
                                <div class = "label-container">
                                    <p class="label-container-first">
                                        BLACKPURL TAX CODE
                                    </p>
                                    <p class="label-container-second text-uppercase" >
                                        {{AccountingSetupModel.selectedIntegrationName}} ONLINE TAX CODE
                                    </p>
                                </div>
                                <div ng-repeat = "salesTaxRec in AccountingSetupModel.selesTaxList" ng-init="salesTaxItemIndex = $index">
                                <p class="DetailLabel">
                                    {{AccountingSetupModel.getTaxCodeMappingLabel(salesTaxRec.SalesTaxName,salesTaxRec.TaxRate)}}
                                </p>
                                <div class="input-group AcoountingDropDown-input-group">
                                    <div class="input-group-btn">
                                        <button class="btn btn-default dropdown-toggle AcoountingDropDown-dropdown-toggle dropDownBtnPB dropDownWidth" 
                                            ng-class="{'inputTextColor': AccountingSetupModel.getTaxCodeMappingNameFromKey(salesTaxItemIndex,false) != 'Select a GL Account'}"
                                            data-toggle="dropdown" type="button" aria-haspopup="true" aria-expanded="false"
                                            ng-if="AccountingSetupModel.QBTaxCodeList.length > 0"
                                            ng-disabled ="!AccountingSetupModel.isValidAllSalesTaxItemMapping"
                                            ng-click="AccountingSetupModel.addOpenClass(salesTaxRec.SalesTaxName);"> 
                                                <span class="fa fa-angle-down dropDownArrorBtnMT"></span>
                                                <p class="tax-code-input"> <!-- 24 march -->
                                                    {{AccountingSetupModel.getTaxCodeMappingNameFromKey(salesTaxItemIndex,false)}}<!-- ng-bind="AccountingSetupModel.getAccountNameFromKey('UndepositedFunds')" -->
                                                </p>
                                        </button> 
                                        <ul class="dropdown-menu AccountingUL keep_open dropDownWidth" role="menu" id="{{AccountingSetupModel.generateId(salesTaxRec.SalesTaxName,'UL')}}">
                                            <div class="searchContainer">
                                                <i class="glyphicon glyphicon-search"></i> 
                                                <input class="form-control" id="{{AccountingSetupModel.generateId(salesTaxRec.SalesTaxName,'Input')}}"
                                                ng-model="salesTaxRec.QBTaxCodeName" 
                                                placeholder="Search" type="search"/>
                                            </div>
                                            <div class="listItemsContainer">
	                                            <ul class="taxCodeRecWrapper">
                                                <li data-value="" 
                                                    ng-repeat="taxCodeRec in AccountingSetupModel.QBTaxCodeList | filter:salesTaxRec.QBTaxCodeName | orderBy:'AccountingId'">
                                                    <a  ng-click="AccountingSetupModel.SelectTaxRec($event,salesTaxItemIndex,taxCodeRec,false,salesTaxRec.SalesTaxName);"
                                                     data-toggle="tooltip" title="{{taxCodeRec.taxCodeLabel}}"   >
	                                                        <p class="taxCodeRec">{{taxCodeRec.Name}}</p>
	                                                        <span class="taxCodeLabel">{{taxCodeRec.taxCodeLabel}} <!-- {{account.AccountingId}} - {{account.AccountName}} --></span>
                                                    </a>
	                                                </li>
	                                             </ul>
                                            </div>
                                        </ul>
                                        <input type="text" class="form-control AcoountingDropDown-dropdown-toggle MT0 inputText" id="{{AccountingSetupModel.generateId(salesTaxRec.SalesTaxName,'')}}" placeholder="Select a GL Account" maxlength="50"
                                            ng-model="AccountingSetupModel.accountingData.defaultAccounts.UndepositedFunds" rel="{{AccountingSetupModel.generateId(salesTaxRec.SalesTaxName,'Id')}}"
                                            data-customtoggle="tooltip" 
                                            ng-if="AccountingSetupModel.QBTaxCodeList.length == 0"
                                            ng-change="AccountingSetupModel.isAccountingSetupDataSaved = false;"/>  <!-- ng-change="AccountingSetupModel.isAccountingSetupDataSaved = false;": 
                                                                                                                                Pooja 9 Mar. 2017 -->
                                        <span class="HelperText BlueTooltip tax-code-mapping-tooltip" id="{{AccountingSetupModel.generateId(salesTaxRec.SalesTaxName,'Tooltip')}}">
                                            <i class="tax-code-warining bp-cursor-pointer red_EB5757 " ng-include="'{!Application_Images}/Icons/warning-filled.svg'"
                                           data-customtoggle="tooltip"  data-original-title="{{salesTaxRec.ErrorMessage}}"></i>
                                        </span>
                                    </div>
                                </div>
                                </div>
                           	</section>
                            </div>
                            <section>
                            <div class="ControlAccountsContainer">
                                <p class="DetailLabel">
                                    Select a {{AccountingSetupModel.selectedIntegrationName}} tax code to use for non-taxable on sales
                                </p>
                                <div class="input-group AcoountingDropDown-input-group">
                                    <div class="input-group-btn">
                                        <button class="btn btn-default dropdown-toggle AcoountingDropDown-dropdown-toggle dropDownBtnPB dropDownWidth" 
                                            ng-class="{'inputTextColor': AccountingSetupModel.getTaxCodeMappingNameFromKey('','',true,'Sales') != 'Select a GL Account'}"
                                            data-toggle="dropdown" type="button" aria-haspopup="true" aria-expanded="false"
                                            ng-if="AccountingSetupModel.nonTaxableTaxCodeList.length > 0"
                                            ng-click="AccountingSetupModel.addOpenClass('nonTaxableTaxCode');"> 
                                                <span class="fa fa-angle-down dropDownArrorBtnMT"></span>
                                                <p style="overflow: hidden; width: 94%;"> <!-- 24 march -->
                                                    {{AccountingSetupModel.getTaxCodeMappingNameFromKey('','',true,'Sales')}}<!-- ng-bind="AccountingSetupModel.getAccountNameFromKey('UndepositedFunds')" -->
                                                </p>
                                        </button> 
                                        <ul class="dropdown-menu AccountingUL keep_open dropDownWidth" role="menu" id="nonTaxableTaxCodeUL">
                                            <div class="searchContainer">
                                                <i class="glyphicon glyphicon-search"></i> 
                                                <input class="form-control" id="nonTaxableTaxCodeInput"
                                                ng-model="AccountingSetupModel.nonTaxableTaxCodeName" 
                                                placeholder="Search" type="search"/>
                                            </div>
                                            <div class="listItemsContainer">
                                                <li data-value="" 
