        List<User> UserRecList = [Select Id, Profile.Name From User where Id In : userIds And Profile.Name = 'BlackPurl' ];
        

        String namespace = [Select NamespacePrefix, Name, Id From ApexClass where Name = 'Constants'].NamespacePrefix;

        List<PackageLicense> packLicRec = [SELECT Id, NamespacePrefix FROM PackageLicense WHERE
                             NamespacePrefix = :namespace];
        
        List<UserPackageLicense> userpackageListToInsert = new List<UserPackageLicense>();
