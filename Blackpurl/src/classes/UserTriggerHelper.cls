    public static Filter__c insertFilter(String  filterLabel, Boolean isFavorite, Boolean isCommon, Boolean isCustom, String UserId) {
        Filter__c filterRec = new Filter__c();
        filterRec.Filter_Label__c = filterLabel;
        filterRec.CommonFilterCategory__c = FILTER_LABEL_TO_COMMON_FILTER_CATEGORY_MAP.get(filterLabel);
        filterRec.favorite__c = isFavorite;
        filterRec.Common__c = isCommon;
        filterRec.Custom__c = isCustom ;
        filterRec.Field_Filter_Json__c = FILTER_LABEL_TO_FIELD_FILTER_JSON_MAP.get(filterLabel);
        filterRec.Sort_Json__c = FILTER_LABEL_TO_SORT_JSON_MAP.get(filterLabel);
        filterRec.Display_Hidden_Column__c = FILTER_LABEL_TO_DISPLAY_HIDDEN_COLUMN_MAP.get(filterLabel);
        filterRec.Additional_Field_Json__c = FILTER_LABEL_TO_ADDITIONAL_FIELD_JSON_MAP.get(filterLabel);
        filterRec.Summary_Field_Json__c = FILTER_LABEL_TO_SUMMARY_FIELD_MAP.get(filterLabel);
        filterRec.Filter_User__c = UserId;
        filterRec.Export_Field_Json__c = filterRec.Summary_Field_Json__c;
        if(FILTER_LABEL_TO_EXPORT_GROUP_BY_FIELD_MAP.keySet().contains(filterLabel)) {
            filterRec.Export_GroupBy_Field__c = FILTER_LABEL_TO_EXPORT_GROUP_BY_FIELD_MAP.get(filterLabel);
        }
        return filterRec;
    }
    
    public static void populateDateFields(){
        for(User userRec : userNewList) {
            if(trigger.isInsert) {
                userRec.CreatedDate__c = system.today();
            }
            userRec.LastModifiedDate__c = system.today();
            
            // Populate Timezone offset value - Used in Time field sorting according to User local
            userRec.Timezone_Offset__c = (System.Timezone.getTimeZone(userRec.TimeZoneSidKey)).getOffset(system.now()) / (1000 * 60 * 60.0); 
        }
        removeInActiveUserFromGroup();
    }
