    <fields>
        <fullName>Per_Item_Profit__c</fullName>
        <externalId>false</externalId>
        <formula>Qty__c * 
IF(
	OR(Part__c != null, Labour_Code__c != null),
	IF(
		$Setup.Configurations__c.Tax_Included_Pricing__c,
		Price_When_Tax_Included__c - Per_Item_Cost__c,
		Price__c - Per_Item_Cost__c
	), 
	IF(
		Fee__c != null,
		IF(
			$Setup.Configurations__c.Tax_Included_Pricing__c,
			Price_When_Tax_Included__c - (IF(ISPICKVAL(Fee__r.Cost_Method__c, &apos;Fixed Cost&apos;), 
														Fee__r.Cost_Rate__c, 
														((Price_When_Tax_Included__c * Fee__r.Cost_Rate__c) / 100)
												)),
			Price__c - (IF(ISPICKVAL(Fee__r.Cost_Method__c, &apos;Fixed Cost&apos;), 
											Fee__r.Cost_Rate__c, 
											((Price__c * Fee__r.Cost_Rate__c) / 100)
									))
		),
		IF(
			Product__c != null,
			IF(
				$Setup.Configurations__c.Tax_Included_Pricing__c,
				Price_When_Tax_Included__c - (IF(ISPICKVAL(Product__r.Cost_Method__c, &apos;Fixed Cost&apos;), 
															Product__r.Cost__c, 
															((Price_When_Tax_Included__c * Product__r.Cost__c) / 100)
													)),
				Price__c - (IF(ISPICKVAL(Product__r.Cost_Method__c, &apos;Fixed Cost&apos;), 
												Product__r.Cost__c, 
												((Price__c * Product__r.Cost__c) / 100)
										))
			),
			0
		)
	)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Per Item Profit</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
